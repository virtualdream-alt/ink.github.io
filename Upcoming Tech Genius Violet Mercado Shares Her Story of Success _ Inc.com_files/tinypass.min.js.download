      _reloadAll(Object.assign(initConfig, { userToken: '' }), exceptIframeId);
    },
    checkThreeDSRedirect: function() {
      _checkThreeDSRedirect();
    }
  };
});

tp.define('template',['jq', 'util', 'postmessage', 'containerUtils', 'user', 'sha1'],
  function ($, util, postmessage, containerUtils, user, sha1) {

    function createTemplateIframe(config) {
      var url = tp.getEndpointRoot(true) + '/checkout/template/show';

      if (tp.cacheableShowTemplate) {
        url = tp.getEndpointRoot(true) + '/checkout/template/cacheableShow';
      }

      config.iframeUrl = url;
      config.postMessageUrl = url;
      return tp.offer.createOfferIframe(config);
    }

    function buildQueryStringFn(iframeParams) {
      var filteredParams = {};
      var filter = ['aid', 'templateId', 'templateVariantId', 'previewTemplateVersion', 'preload',
        'preview', 'previewUncached', 'deploymentItemId', 'gaClientId', 'zuoraSelectedRatePlanIds',
        'newscycleSelectedIds', 'addNewsletterAssets', 'offerId', 'displayMode', 'experienceId', 'url', 'width', 'iframeId'];

      $.each(filter, function (index, value) {
        if (iframeParams[value]) {
          filteredParams[value] = iframeParams[value];
        }
      });

      var queryString = $.param(filteredParams, true);
      return queryString;
    }

    function generateIframeIdFn(config, iframeCounterByUrlMap) {
      var counter = 0;

      var hash = "offer_" + sha1.hash(config.iframeUrl + '?' + buildQueryStringFn(config), 20);
      if (iframeCounterByUrlMap.has(hash)) {
        counter = iframeCounterByUrlMap.get(hash) + 1;
      }
      iframeCounterByUrlMap.set(hash, counter);
      return hash + '-' + counter;
    }

    function initCacheableTemplate(config) {
       config.trackStatsOnShow = false;
       config.deferredContextLoading = true;
       config.buildQueryStringFn = buildQueryStringFn;
       config.generateIframeIdFn = generateIframeIdFn;
    }

    return {
      name: 'template',
      /**
       * Shows an template on the page.
       *
       * @param config
       * The parameters that can get specified are:
       *
       *     templateId (required):
       *         The id of the template being loaded, found in the publisher dashboard
       *
       *     displayMode (optional; default=modal):
       *         Configures the display mode of the template. Valid options
       *         are: inline, modal. Inline requires that a containerSelector is specified.
       *
       *         When set to modal, the semi-transparent backdrop will appear and the
       *         offer and template will appear in an iframe in the middle of the browser.
       *
       *         When set to inline, the iframe will get loaded in the
       *
       *     containerSelector (conditionally required):
       *         The jQuery selector that targets a single DOM element on the page that the
       *         template should appear in. For example a containerSelector of
       *         ".tp-template-form" should be used if there were one, and only one:
       *
       *             <div class="tp-template-form"></div>
       *
       *         that resided on the page that the template should appear in.
       *
       *         This option is only used when displayMode is inline, and must be a valid
       *         selector for a single DOM element on the page.
       *
       *     close (optional):
       *         A callback that fires when the user dismisses a modal. This event only fires
       *         when the user dismisses an iframe that is in modal mode.
       *
       *     customEvent (optional):
       *         A callback that fires when the user performs an action in the template that
       *         the publisher has configured to listen for.
       *
       *         For example, a publisher might implement code on their website such as:
       *
       *         tp.template.show({
       *             // all required fields, plus
       *             customEvent : function(event) {
       *                 switch(event.params.eventName) {
       *                     case "login":
       *                         window.location.href = "/login";
       *                         break;
       *                 }
       *             }
       *         });
       *
       *         In their template, their markup would contain:
       *
       *             <a href="#" ng-click="customEvent('login')">Login</a>
       *
       *         This callback would get fired when the user clicked Login from within template.
       *
       */
      show: function (config) {
        // TODO implement without using offer.js
        config.offerId = containerUtils.fakeOfferId;
        config.createIframeFn = createTemplateIframe;
        config.trackStatsOnShow = true;
        if (tp.cacheableShowTemplate) {
          initCacheableTemplate(config);
        }
        return tp.offer.show(config);
      },
      deferredShow: function (config) {
        config.offerId = containerUtils.fakeOfferId;
        config.createIframeFn = createTemplateIframe;
        if (tp.cacheableShowTemplate) {
          initCacheableTemplate(config);
        }
        return tp.offer.deferredShow(config);
      },
      close: function () {
        containerUtils.closeModalContainer();
      },
      closeInline: function (selector) {
        containerUtils.__protected__.closeInlineContainers(selector);
      },
      createTemplateIframe: function (config) {
        // TODO implement without using offer.js
        return createTemplateIframe(config);
      },
      showBackdrop: function () {
        containerUtils.showBackdrop();
      },
      hideBackdrop: function () {
        containerUtils.hideBackdrop();
      },
      centerBackdrop: function (container) {
        containerUtils.centerBackdrop(container);
      },
      registerCallback: function (callbackName, callback) {
        // TODO implement when we will use own event manager
        //eventManager.addEvent(callbackName);
        //eventManager.registerCallback(callbackName, callback);
      },
      refreshUser: function (iframeId) {
        user.refreshAccessToken(true, function () {
          postmessage.send('externalLoginSuccess', {
            user_token: user.getProvider().getToken()
          }, iframeId);
        });
      }
    };
  });

tp.define('myaccount',['jq', 'util', 'postmessage', 'offer', 'user', 'containerUtils', 'pianoId'],
  function ($, util, postmessage, offer, user, containerUtils, pianoId) {
    var VERIFICATION_CODE_QUERY = 'verification_code';

    var iframes = {};

    var tooTall = false;

    var callbacks = {
      loginRequired: [],
      loginSuccess: []
    };

    var _getBaseUrl = function () {
      return tp.getEndpointRoot() + '/checkout/myaccount/show';
    };

    var _createContainer = function () {
      var container = $(document.createElement('div')).addClass('tp-modal');
      var close = $(document.createElement('div')).addClass('tp-close');
      close.on('click', _closeContainer);
      container.append(close);
      $('body').append(container);
      return container;
    };

    var _resizeIframe = function (iframe, params) {
      iframe.element.css('height',
        (iframe.config.maxHeight ?
          Math.min(params.height, iframe.config.maxHeight)
          : params.height) + 'px');
      iframe.container.css('height',
        (iframe.config.maxHeight ?
          Math.min(params.height, iframe.config.maxHeight)
          : params.height) + 'px');
      if (iframe.config.displayMode === 'modal') {
        iframe.element.css('width', params.width + 'px');
        iframe.container.css('width', params.width + 'px');
      }
      postmessage.send('resized', {
        height: (iframe.config.maxHeight ?
          Math.min(params.height, iframe.config.maxHeight)
          : params.height),
        width: params.width
      }, iframe.iframeId);
    };

    var dialog = null;

    var _createDialog = function (params, onClose) {
      var container = $(document.createElement('div')).addClass('tp-modal');
      var close = $(document.createElement('div')).addClass('tp-close');
      var message = $(document.createElement('div')).addClass('tp-message');
      container.css('width', (params.width ? params.width : 400) + 'px');

      message.html(params.message);

      var btnContainer = $(document.createElement('div')).addClass('tp-btn-container');

      var okBtn = $(document.createElement('button')).addClass('tp-btn').addClass('tp-ok-btn');
      okBtn.html(params.ok ? params.ok : 'Ok');
      var cancelBtn = $(document.createElement('button')).addClass('tp-btn').addClass('tp-cancel-btn');
      cancelBtn.html(params.cancel ? params.cancel : 'Cancel');

      close.on('click', _closeDialog(onClose, params, 'cancel'));
      cancelBtn.on('click', _closeDialog(onClose, params, 'cancel'));
      okBtn.on('click', _closeDialog(onClose, params, 'ok'));

      container.append(close);
      container.append(message);
      btnContainer.append(okBtn);
      btnContainer.append(cancelBtn);
      container.append(btnContainer);

      $('body').append(container);

      offer.showBackdrop();
      container.addClass('tp-active');
      _centerIt(container);

      dialog = container;
    };

    var _closeDialog = function (onClose, params, result) {
      return function () {
        if (dialog) {
          dialog.removeClass('tp-active');
          offer.hideBackdrop();
          window.setTimeout(function () {
            dialog.remove();
          }, 250);
          params.result = result;
          onClose(params);
        }
      };
    };

    function _centerIt(container) {
      if (container.outerHeight() > $(window).height()) {
        if (tooTall === false) {
          container.css({ 'position': 'absolute' });
          tooTall = true;
        }
        container.css({ marginTop: $(window).scrollTop() - ($(window).height() * .5) });
      } else {
        if (tooTall === true) {
          container.css({ 'position': 'fixed' });
          tooTall = false;
        }
        container.css({ marginTop: 0 - (container.outerHeight() * .5) });
      }
      offer.centerBackdrop(container);
      container.css({ marginLeft: 0 - (container.outerWidth() * .5) });
    }


    var _openDialog = function (event, params) {
      _createDialog(params.params, function (result) {
        postmessage.send('closeDialog', result, iframes[params.sender].iframeId);
      });
    };

    /**
     * This registers a callback for a certain event. When _fireCallbacks() is
     * called, every callback that has been registered via this method will get
     * called.
     *
     * @param event     The event name, e.g. onMeterExpired
     * @param callback  The callback function to be executed
     * @private
     */
    var _registerCallback = function (event, callback) {
      callbacks[event] = callbacks[event] || [];
      callbacks[event].push(callback);
    };

    /**
     * Fires events for a specific callback. Will call every callback registered
     * using _registerCallback()
     *
     * @param event   The event that has fired
     * @param object  An object to pass to the callback
     * @private
     */
    var _fireCallbacks = function (event, object, iframeId) {
      util.debug('received event: ' + event, object);
      if (typeof (callbacks[event]) === 'undefined') {
        return;
      }
      for (var i = 0; i < callbacks[event].length; ++i) {
        util.debug('firing callback for event: ' + event);
        var result = callbacks[event][i](object);

        if (object && object.resultCallbackId) {
          object.result = result;
          object.event = event;
          postmessage.send('resultCallback', object, iframeId);
        }
      }

      if (!callbacks[event].length && object && object.resultCallbackId) {
        object.callbackNotFound = true;
        object.event = event;
        postmessage.send('resultCallback', object, iframeId);
      }
    };

    var _receive = function (params) {
      util.log('*** My account event received', params.event, params.params);
      switch (params.event) {
        case 'resize':
          util.debug('*** Resize event', params.params.width, params.params.height);
          _resizeIframe(iframes[params.sender], params.params);
          if (iframes[params.sender].config.displayMode === 'modal') {
            _centerIt(iframes[params.sender].container);
          }
          break;
        case 'startCheckout':
          _startCheckout(params.params);
          break;
        case 'loaded':
          _resizeIframe(iframes[params.sender], params.params);

          if (user.getProvider().name === 'pianoId') {
            tp.pianoId.__protected__.executePostMessagesBatch();
          }

          if (iframes[params.sender].config.displayMode === 'modal') {
            backdrop.show();
            window.setTimeout(function () {
              backdrop.addClass('tp-active');
              iframes[params.sender].container.addClass('tp-active');
              _centerIt(iframes[params.sender].container);
            }, 25);
          }
          break;
        case 'close':
          // _closeContainer();
          break;
        case 'cancel':
        case 'complete':
        case 'error':
        case 'loginRequired':
        case 'customEvent':
          _fireCallbacks(params.event, params.params, params.sender);
          break;
        case 'openDialog':
          _openDialog(params.event, params);
          break;
        case 'pianoIdProfileUpdated':
          pianoId.onProfileUpdate(params.params, function () {
            var object = params.params;
            if (object && object.resultCallbackId) {
              postmessage.send('resultCallback', object, params.sender);
            }
          });
          break;
        case 'extendedUserLoginRequired':
          pianoId.onExtendedUserLoginRequired();
          break;
        case 'loginSuccess':
          user.getProvider().onLoginSuccess(params.params);

          var config = {
            reloadAfterLogin: true,
            userToken: params.params.user_token,
            user: params.params.user
          };
          _reload(config);
          _fireCallbacks(params.event, params.params, params.sender);
          break;
        case 'logout':
          user.logout(function () {
            var config = {};
            config.userProvider = user.getProvider().getName();
            config.userToken = user.getProvider().getToken() && user.getProvider().getToken().length > 0 ? user.getProvider().getToken() : '';
            _reload(config);
          });
          _fireCallbacks(params.event, params.params, params.sender);
          break;
        case 'userProfileUpdateSuccess':
          var message = params;

          var messageEvent = message.event;
          var messageParams = message.params;
          var messageSender = message.sender;

          user.getProvider().onLoginSuccess(messageParams);

          var reloadConfig = {
            userProvider: messageParams.user_provider,
            userToken: messageParams.user_token
          };

          _reload(reloadConfig);

          _fireCallbacks(messageEvent, messageParams, messageSender);

          for (var key in iframes) {
            if (iframes.hasOwnProperty(key)) {
              var iframe = iframes[key];

              var iframeConfig = iframe.config;

              if (iframeConfig.hasOwnProperty('userToken')) {
                delete iframeConfig['userToken'];
              }

              _show(iframeConfig);
            }
          }

          break;
        case 'langChangedWithinMyAccount':
          if (tp.pianoId.isConfigured()) {
            tp.pianoId.updateLang(params.params.lang);
          }
          break;
        case 'requestScrollData':
          _returnScrollData(params.sender);
          break;
        case 'downForMaintenance' :
          _resizeIframe(iframes[params.sender], { width: 300, height: 200 });
          break;
        case "removeQueryParamByName":
          if (Array.isArray(params.params.paramsToRemove)) {
            params.params.paramsToRemove.forEach(function (name) {
              util.removeQueryParamByName(name);
            });
          }
          break;
        case "getTrackingData":
          postmessage.send('getTrackingDataResponseEvent', { pageViewId: tp.main.fetchPageViewId(), tbc: util.getTbcCookie() }, params.sender);
          break;
        default:
          util.log('Received invalid event type: ' + params.event);
          return;
      }
    };

    var _reload = function (config) {
      postmessage.broadcast('reloadOffer', config);
    };

    var _handleKeystroke = function (event) {
      if (event.keyCode === 27 && $.map(iframes, util.mapArrayIndex).length > 0) {
        _closeContainer();
      }
    };

    var _returnScrollData = function(senderId) {

      var myAccountRect = iframes[senderId].element[0].getBoundingClientRect();
      var data = {
        iframeRect: {
          // `ClientRect` object is not passed directly because in Edge it becomes an empty
          // object when passed via post message.
          bottom: myAccountRect.bottom,
          height: myAccountRect.height,
          left: myAccountRect.left,
          right: myAccountRect.right,
          top: myAccountRect.top,
          width: myAccountRect.width
        }
      };

      postmessage.send('returnScrollData', data, senderId);
    };

    var _checkIframeSizes = function () {
      for (var iframeId in iframes) {
        var iframe = iframes[iframeId];
        var needToSend = false;
        if (iframe.container.width() !== iframe.containerWidth) {
          iframe.element.css('width', iframe.container.width());
          iframe.containerWidth = iframe.container.width();
          needToSend = true;
        }
        if (iframe.container.height() !== iframe.containerHeight) {
          iframe.element.css('height', iframe.container.height());
          iframe.containerHeight = iframe.container.height();
          needToSend = true;
        }
        if (needToSend) {
          postmessage.send('resize', {
            width: iframe.container.width(),
            height: iframe.container.height()
          }, iframeId);
        }
      }
    };

    var _createIframe = function (config) {
      var iframeParams = {};

      if (config.templateId) {
        iframeParams.templateId = config.templateId;
      }

      if (typeof (config.displayMode) === 'undefined') {
        config.displayMode = 'inline';
      } else {
        if (config.displayMode !== 'modal' && config.displayMode !== 'inline') {
          util.log('Invalid offer displayMode; must be one of: modal, inline');
          return;
        }
      }
      iframeParams.displayMode = config.displayMode;
      iframeParams.iframeId = util.randomString(16);
      iframeParams.url = window.location.href;
      iframeParams.initialWidth = config.width;
      iframeParams.initialHeight = config.height;
      iframeParams.maxHeight = config.maxHeight;

      if(tp.locale){
        iframeParams.lang = tp.locale;
      }


      if (tp.janrain.isConfigured() && (typeof (config.loginRequired) === 'undefined' || !config.loginRequired)) {
        var hiddenIframes = [];
        var savedOfferConfig = config;

        tp.janrain.addEventHandler('onModalClose', function () {
          containerUtils.activateContainers(hiddenIframes);
          util.doneCall('myAccountLogin');
        });

        tp.janrain.registerCallback('userChanged', function (result) {
          if (result && result.user_token) {
            var loginSuccessResult;
            if (config.loginSuccess) {
              loginSuccessResult = config.loginSuccess(result);
            }
            if (typeof (loginSuccessResult) === 'undefined' || loginSuccessResult) {
              savedOfferConfig.userToken = result.user_token;
              _show(savedOfferConfig);
            }
          }
        });

        tp.janrain.addEventHandler('onCaptureSessionEnded', function (result) {
          var loginSuccessResult;
          if (config.loginSuccess) {
            loginSuccessResult = config.loginSuccess(result);
          }
          if (typeof (loginSuccessResult) === 'undefined' || loginSuccessResult) {
            savedOfferConfig.userToken = null;
            _show(savedOfferConfig);
          }
        });

        config.loginRequired = function () {
          // util.waitCall("pageTrack", function () {
          util.waitCall('refreshAccessTokenList', function () {
            util.waitCall('offerCreate', function () {
              util.startCall('myAccountLogin');

              hiddenIframes = containerUtils.hideModalContainer();
              tp.janrain.login();
            });
          });
          // });

          return false;
        };
      } else if (tp.gigya.isConfigured() && (typeof (config.loginRequired) === 'undefined' || !config.loginRequired)) {
        var hiddenIframes = [];

        tp.gigya.registerCallback('onHide', function () {
          containerUtils.activateContainers(hiddenIframes);
          util.doneCall('myAccountLogin');
        });

        config.loginRequired = function () {
          var savedOfferConfig = config;
          util.waitCall('refreshAccessTokenList', function () {
            util.waitCall('offerCreate', function () {
              util.startCall('myAccountLogin');
              hiddenIframes = containerUtils.hideModalContainer();
              tp.gigya.login(function (result) {
                var loginSuccessResult;
                if (config.loginSuccess) {
                  loginSuccessResult = config.loginSuccess(result);
                }
                if (typeof (loginSuccessResult) === 'undefined' || loginSuccessResult) {
                  savedOfferConfig.userToken = result.user_token;
                  _show(savedOfferConfig);
                }
              });
            });
          });
        };
      } else if (tp.pianoId.isConfigured()) {
        var savedOfferConfig = config;
        tp.pianoId.setCallbacks({
          loggedIn: function (result) {
            if (iframeParams.userToken === result.token) {
              return;
            }
            var loginSuccessResult;
            if (config.loginSuccess) {
              loginSuccessResult = config.loginSuccess(result);
            }
            if (typeof (loginSuccessResult) === 'undefined' || loginSuccessResult) {
              savedOfferConfig.userToken = result.token;
              _show(savedOfferConfig);
            }
          },
          loggedOut: function () {
            var config = {};
            config.userProvider = user.getProvider().getName();
            config.userToken = user.getProvider().getToken() && user.getProvider().getToken().length > 0 ? user.getProvider().getToken() : '';
            _reload(config);
          }
        });
        if ((typeof (config.loginRequired) === 'undefined' || !config.loginRequired)) {
          var countOfLogins = 0;
          config.loginRequired = function () {
            countOfLogins++;
            if (countOfLogins > 10) {
              util.log('Too many attemps to login for myAccount');
              return;
            }

            var verificationCode = util.getQueryParamByName(VERIFICATION_CODE_QUERY);

            tp.pianoId.show({ verificationCode: verificationCode });
          };
        }
        iframeParams.pianoIdUrl = tp.pianoId.getIframeUrl();
        var lang = tp.pianoId.getLang();
        if (lang) {
          iframeParams.lang = lang;
        }
        tp.pianoId.onLangChange(function (lang) {
          postmessage.send('langChanged', {lang: lang}, iframeParams.iframeId);
        }, iframeParams.iframeId);
      } else if (tp.pianoIdLite.isConfigured()) {
        iframeParams.pianoIdUrl = tp.pianoId.getIframeUrl();
      }
      var def = $.Deferred();

      util.waitCall('refreshUserToken', function () {
        var container;
        if (config.displayMode === 'inline') {
          if (!config.containerSelector) {
            util.log('My account must have a valid containerSelector');
            return;
          }
          if ($(config.containerSelector).length === 0) {
            util.log('My account must have a valid containerSelector');
            return;
          }
          container = $(config.containerSelector);
          iframeParams.width = container.width();
        } else {
          // create backdrop if it doesn't exist
          container = _createContainer();
        }

        iframeParams.userProvider = user.getProvider().getName();
        if (config.userToken) {
          iframeParams.userToken = config.userToken;
        } else {
          iframeParams.userToken = user.getProvider().getToken();
        }

        for (var cb in callbacks) {
          if (typeof (config[cb]) === 'function') {
            util.debug('Registering ' + cb + ' callback for iframe ' + iframeParams.iframeId);
            _registerCallback(cb, config[cb]);
          }
        }
        iframeParams.aid = tp.aid;

        var iframeSrc = _getBaseUrl() + '?' + $.param(iframeParams, true);
        var iframe = $(document.createElement('iframe'));

        iframe.attr('scrolling', 'no');
        iframe.attr('allowtransparency', 'true');
        iframe.css('overflow', 'hidden');
        iframe.css('border', '0');
        iframe.css('height', '0px');
        iframe.attr('src', iframeSrc);
        iframe.attr('id', iframeParams.iframeId);

        if (config.displayMode === 'inline') {
          iframe.css('width', iframeParams.width + 'px');
        } else {
          iframe.css('width', '0px');
        }

        iframes[iframeParams.iframeId] = {
          iframeId: iframeParams.iframeId,
          config: config,
          element: iframe,
          container: container,
          containerWidth: container.width(),
          containerHeight: container.height()
        };
        postmessage.init({
          postMessageUrl: _getBaseUrl(),
          receive: _receive,
          iframe: iframes[iframeParams.iframeId]
        });
        $(window).on('keyup', _handleKeystroke);

        if (config.displayMode === 'modal') {
          $(window).resize(function () {
            _centerIt(container);
          }.bind(this));
        }
        if (config.displayMode === 'modal') {
          _centerIt(container);
          container.append(iframe);
        } else {
          container.html(iframe);
        }

        $(window).resize(_checkIframeSizes);

        if (tp.offer) {
          tp.offer.registerCallback('complete', function () {
            postmessage.send('refreshNow', {}, iframeParams.iframeId);
          });
        }
        def.resolve(iframes[iframeParams.iframeId]);
      });
      return def.promise();
