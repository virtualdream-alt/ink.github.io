    var LANG_COOKIE_NAME = '__pil';
    var DOMAIN_COOKIE_NAME = '__pid';
    var REMEMBER_ME_COOKIE_NAME = '__idr';
    var EXTEND_EXPIRED_ACCESS_COOKIE_NAME = '__eea';
    var containerInner = 'piano-id-container';
    var PIANO_ID_SOURCE = 'PIANOID';
    var DEFAULT_PIANOID_ORIGIN = 'https://id.tinypass.com';
    var SOURCE_CUSTOM_FORM = 'show_form';
    var SOURCE_REGISTRATION = 'registration';
    var SOURCE_CHECKOUT = 'checkout';
    var REQUEST_TIMEOUT = 300;
    var FORCE_CLOSE_IFRAME_TIMEOUT = 600;
    var globalConfig = {};
    var iframes = [];
    var iframeModal;
    var localCallbacks = {
      loggedIn: [],
      loggedOut: [],
      loginSuccess: [],
      registrationSuccess: [],
      profileUpdate: [],
      langChange: [],
      langChangeFromIframe: {},
      fire_loginSuccess: [],
      loginDisplayed: [],
      registerDisplayed: [],
      loginFailed: [],
      registrationFailed: [],
      onShow: [],
      layoutReady: []
    };
    var customEvents = {};
    var globalCallbacks = {};
    var initPromise;
    var configPromise;
    var isNewUser;
    var loginSuccessInProgress;
    var postMessagesBatch = [];

    var aidsForNewPianoIdVersion = [
      // add publisher aid here for new piano id version
      'G18rIJWQ2G',
      '45L2BaS6pu' // the spectator
    ];

    $(window).resize(function () {
      return checkResize();
    });

    function checkResize(elements) {
      iframes
        .filter(function (iframe) {
          return iframe.config.displayMode === 'inline';
        })

        .filter(function (iframe) {
          return !elements ||
            elements.some(function (element) {
              return iframe.container.is(element);
            });
        })
        .forEach(function (iframe) {

          containerUtils.resizeIframe(iframe, {
            width: iframe.container.width()
          });

          postmessage.send('resize', { width: iframe.container.width() }, iframe.iframeId);

        });
    }

    function setAccessToken(accessToken, remember) {
      var user = jwtDecode(accessToken);
      user.rememberMe = !!remember;
      storeToken({
        user: user,
        token: accessToken
      });
    }

    function parseConfig(config) {
      if (configPromise && !config) {
        return configPromise;
      }
      globalConfig = config || {};
      var dfd = $.Deferred();
      configPromise = dfd.promise();

      if (!pianoIdLite.isConfigured()) {
        tp.setUsePianoIdUserProvider(true);
      }

      setCallbacks(globalConfig);
      var resetToken = util.getQueryParamByName(RESET_TOKEN_QUERY);
      if (globalConfig.resetPasswordToken === undefined && resetToken) {
        globalConfig.resetPasswordToken = resetToken;
      }

      var passwordlessToken = util.getQueryParamByName(PASSWORDLESS_TOKEN_QUERY);
      if (globalConfig.passwordlessToken === undefined && passwordlessToken) {
        globalConfig.passwordlessToken = passwordlessToken;
      }

      var access_token = util.getQueryParamByName(ACCESS_TOKEN);
      if (access_token) {
        util.removeQueryParamByName(ACCESS_TOKEN);
        if (!isUserValid()) {
          tp.inappBrowserReturn = true;
          setAccessToken(access_token, true);
        }
      }
      setTimeout(dfd.resolve, 50);
      return configPromise;
    }

    function init(config, withoutAuthByCookie) {
      if (initPromise && !config) {
        return initPromise;
      }

      var dfd = $.Deferred();
      initPromise = dfd.promise();

      if (config && config.preview) {
        setTimeout(dfd.resolve, 10);
        return initPromise;
      }
      // validating current token(from id.piano.io cookie) or obtaining access token from SSO using __preut cookie
      util.startCall('refreshUserToken'); // blocking experience until user check

      parseConfig(config)
        .then(function () {
          if (globalConfig.passwordlessToken) {
            return authByPasswordless();
          } else if (!withoutAuthByCookie){
            return authByCookie();
          } else {
            return $.when();
          }
        })
        .then(doneAndResolve, doneAndResolve);

      function doneAndResolve() {
        util.doneCall('refreshUserToken'); // unblocking experience after user check
        dfd.resolve();
      }

      return initPromise;
    }

    function prepare() {
      return parseConfig();
    }

    function show(config) {
      init().then(function () {
        var showConfig = $.extend({}, globalConfig, config, {
          isRegistration: true
        });
        if (isUserValid()) {
          _fireLoggedIn(showConfig);
        } else if (iframeModal) {
          setCallbacks(config);
          util.log('Modal login window already shown');
        } else {
          setCallbacks(config);
          prepareConfig(showConfig);
          createIframe(showConfig);
        }
      });
    }

    function checkAndShowEmailIsNotConfirmed() {
      var EMAIL_IS_NOT_CONFIRMED = 'email_is_not_confirmed';

      /** @type {'login' | 'registration' | ''} */
      var emailIsNotConfirmedType = util.getQueryParamByName(EMAIL_IS_NOT_CONFIRMED);
      if (!emailIsNotConfirmedType) return;

      util.removeQueryParamByName(EMAIL_IS_NOT_CONFIRMED);

      const screen = emailIsNotConfirmedType === 'registration' ? 'email_is_not_confirmed' : 'email_confirmation_required';
      var user = getUser();
      _showCustom({screen: screen, email: user && user.email});
    }

    function showEmailConfirmationRequired(config) {
      if (iframeModal) {
        util.log('Modal window already shown');
        return;
      }

      init().then(function () {
        var showConfig = $.extend({ screen: 'email_confirmation_required' }, globalConfig, config);
        setCallbacks(config);
        prepareConfig(showConfig);
        createIframe(showConfig);
      });
    }

    function showEmailConfirmation(config) {
      if (iframeModal) {
        util.log('Modal window already shown');
        return;
      }
      init().then(function () {
        var showConfig = $.extend({ screen: 'email_confirmation' }, globalConfig, config);
        setCallbacks(config);
        prepareConfig(showConfig);
        createIframe(showConfig);
      });
    }

    /**
     * shows iframe with extended config
     * @param config
     * @private
     * @example with customParams
     * _showCustom({
     *   screen: 'passwordless_confirmation',
     *   customParams: {
     *     resourceName: 'scratta du forlurar du'
     *   }
     * });
     *
     * @example with customEvents
     * _showCustom({
     *   screen: 'passwordless_confirmation',
     *   customEvents: {
     *     checkoutContinue: function() {
     *       console.log('ylyl');
     *     }
     *   }
     * });
     *
     * @example resend email modal
     * _showCustom({
     *   screen: 'passwordless_confirmation',
     *   email: 'test@piano.io'
     * });
     */
    function _showCustom(config) {
      if (iframeModal) {
        util.log('Modal window already shown');
        return;
      }

      if (!config) {
        config = {};
      }

      var showConfig = $.extend({}, config, globalConfig);
      if (config.customEvents && typeof config.customEvents === 'object') {
        customEvents = config.customEvents;
      }
      prepareConfig(showConfig);
      setCallbacks(config);
      createIframe(showConfig);
    }

    function showForm(config) {
      init().then(function () {
        var showConfig = $.extend({}, globalConfig, config, {
          isRegistration: false
        });

        var formName = showConfig.formName;

        if (!formName) {
          throw new Error('Form name should be specified, like\ntp.pianosId.showForm({formName: "..."});');
        }

        var startShowForm = function () {

          if (iframeModal) {
            util.log('Modal login window already shown');
          } else {
            prepareConfig(showConfig);
            setCallbacks(config);
            if (!showConfig.preview && isNewUser) {
              util.log('Skip show form, register new user');
              return;
            }

            if (!showConfig.hideIfComplete) {
              $.extend(showConfig, {
                readyCallback: function (iframe) {
                  postmessage.send('setToken', getToken(), iframe.iframeId);
                }
              });
              return createIframe(showConfig);
            }

            getUserInfo(showConfig)
              .then(function (data) {

                if (data && data.has_all_custom_field_values_filled && showConfig.hideIfComplete) {
                  util.log('Skip show form, all fields are completed');
                  return;
                }

                var formConfig = $.extend({}, showConfig, {
                  readyCallback: function (iframe) {
                    postmessage.send('userData', data, iframe.iframeId);
                  }
                });

                createIframe(formConfig);

              });
          }
        };
        if (!config.preview && !isUserValid()) {
          var authConfig = $.extend({}, config, {
            closedCallback: function () {
              if (isUserValid()) {
                startShowForm();
              }
            }
          });

          show(authConfig);
        } else {
          startShowForm();
        }
      });
    }

    function submitLoginForm() {
      postmessage.send('submitLoginForm', null, iframeModal.iframeId);
    }

    function authByPasswordless() {
      var dfd = $.Deferred();

      $.ajax({
        url: getPianoIdUrl() + 'api/v1/identity/passwordless/authorization/code',
        dataType: 'jsonp',
        data: {
          aid: globalConfig.aid || tp.aid,
          passwordless_token: globalConfig.passwordlessToken
        },
        success: function (resp) {
          if (!resp.error) {
            _onPasswordlessLoginSuccess(resp);
            dfd.resolve();
          } else {
            _onPasswordlessLoginError(resp);
            dfd.reject();
          }
        },
        xhrFields: {
          withCredentials: true
        },
        crossDomain: true
      });

      return dfd.promise();
    }

    function _onPasswordlessLoginSuccess(resp) {
      if (!resp.error && resp.access_token) {
        _completeCheckout(resp);

        setToken({
          source: PIANO_ID_SOURCE,
          token: resp.access_token,
          user: jwtDecode(resp.access_token),
          extendExpiredAccessEnabled: resp.extend_expired_access_enabled
        }, false, true);

        delete globalConfig['passwordlessToken'];
        util.removeQueryParamByName('passwordless_token');
      }
      util.doneCall('refreshUserToken');
    }

    function _onPasswordlessLoginError(resp) {
      _showCustom({
        screen: 'passwordless_confirmation',
        jsonpError: resp
      });

      delete globalConfig['passwordlessToken'];
      util.removeQueryParamByName('passwordless_token');
    }

    function _completeCheckout(resp) {
      var data = {
        aid: globalConfig.aid || tp.aid,
        login_token_id: resp.login_token_id,
        user_token: resp.access_token
      };

      var additionalConfig = {
        passwordlessToken: resp.passwordless_token,
        redirectUri: resp.direction_url
      };

      _sendCompleteRequest(data, additionalConfig);
    }

    function _sendCompleteRequest (data, additionalConfig) {
      containerUtils.showWaitPanel();
      api.callApi('/anon/passwordless/complete', data, function (resp) {
        if (resp.result && resp.result.polling_enabled) {
          return checkPolling(resp)
            .then(function(response) {
              completeCheckoutCallback(response, additionalConfig);
            });
        }

        completeCheckoutCallback(resp, additionalConfig);
      });
    }

    function checkPolling(response) {
      var defer = $.Deferred();
      var pollingRequestParams = {
        aid: tp.aid,
        oid: response.result.oid,
        process_id: response.result.process_id,
        user_token: tp.user.getProvider().getToken()
      };

      sendPollingRequest(pollingRequestParams, 0, response.result.polling_timeouts, defer.resolve);

      return defer.promise();
    }

    function sendPollingRequest(requestParams, attempts, timeouts, callback) {
      api.callApi('/anon/passwordless/check', requestParams, function (response) {
        attempts++;
        if (response && response.result && response.result.poll_status !== 'waiting') {
          callback(response);
        } else if (!response.result && response.message) {
          util.error('Passwordless purchase failed', response.message);
          callback();
        } else if (attempts <= timeouts.length) {
          setTimeout(function () {
            sendPollingRequest(requestParams, attempts, timeouts, callback);
          }, timeouts[attempts - 1]);
        } else {
          callback();
        }
      });
    }

    function completeCheckoutCallback(response, additionalConfig) {
      try {
        var resource = response.result.resource || {};
        var showOfferParams = JSON.parse(response.result.show_offer_params);
        var config = getCustomScreenConfig(additionalConfig);

        if (!Object.keys(resource).length && !Object.keys(showOfferParams).length) {
          return _showCustom(config);
        }

        if (resource.name) {
          config = getCustomScreenConfig(additionalConfig, {
            customParams: {
              resourceName: resource.name
            }
          });
        } else {
          config = getCustomScreenConfig(additionalConfig, {
            customEvents: {
              checkoutContinue: function () {
                closeModalContainer();
                containerUtils.closeModalContainer();
                tp.offer.show(showOfferParams);
              }
            }
          });
        }

        _showCustom(config);
      } catch (e) {
        _showCustom(getCustomScreenConfig(additionalConfig));
      } finally {
        containerUtils.hideWaitPanel();
      }
    }

    function getCustomScreenConfig(additionalConfig, config) {
      var defaultConfig = {
        screen: 'passwordless_confirmation',
      };

      if (additionalConfig) {
        defaultConfig.passwordlessToken = additionalConfig.passwordlessToken;
        defaultConfig.redirectUri = additionalConfig.redirectUri;
      }

      if (config) {
        return Object.assign(defaultConfig, config);
      }

      return Object.assign(defaultConfig, { passwordlessSuccess: true });
    }

    function _fireLoggedIn(config, source) {
      var user = getUser();
      if (!user) {
        return;
      }
      var token = loadToken();
      var data = {
        token: token,
        user: user,
        source: source
      };
      if (config) {
        config.loggedIn && config.loggedIn(data);
        return;
      }
      _fireLocalCallbacks('loggedIn', data);
      _fireLocalCallbacks('loginSuccess', data);
      if (source === PIANO_ID_SOURCE) {
        var params = {
          event: 'loginSuccess',
          source: source,
          params: data.user,
          user_token: data.token,
          registration: isNewUser
        };
        _fireLocalCallbacks('fire_loginSuccess', params);
        _fireGlobalCallbacks('loginSuccess', params);
      }
    }

    function _fireLoggedOut(source) {
      var data = {
        source: source
      };
      _fireLocalCallbacks('loggedOut', data);
      _fireGlobalCallbacks('logout', data);
    }

    function _fireChangeScreen(screen, onlyGlobal) {
      var eventNamesMap = {
        login: 'loginDisplayed',
        register: 'registerDisplayed'
      };
      var eventName = eventNamesMap[screen];
      _fireCallbacks(eventName, {}, onlyGlobal);
    }

    function _fireProfileUpdate(data) {
      _fireLocalCallbacks('profileUpdate', data);
    }

    function _fireCustomEvent(config, params) {
      config && config.customEvent && config.customEvent(params);
      _fireGlobalCallbacks('customEvent', params);
    }

    function _fireLocalCallbacks(event, params) {
      localCallbacks[event] && localCallbacks[event].forEach(function (callback) {
        callback(params);
      });
    }

    function _fireGlobalCallbacks(event, params) {
      globalCallbacks[event] && globalCallbacks[event].forEach(function (callback) {
        callback(params);
      });
    }

    function _fireCallbacks(event, params, onlyGlobal) {
      !onlyGlobal && _fireLocalCallbacks(event, params);
      _fireGlobalCallbacks(event, params);
    }

    function _fireCustomEvents(name) {
      if (customEvents && typeof customEvents[name] === 'function') {
        customEvents[name]();
      }
    }

    function setCallbacks(config) {
      if (!config) {
        return;
      }
      Object.keys(localCallbacks).forEach(function (eventName) {
        config[eventName] && localCallbacks[eventName].push(config[eventName]);
      });
    }

    function prepareConfig(config) {
      if (!config.aid) {
        config.aid = tp.aid;
      }

      if (!config.displayMode) {
        config.displayMode = 'modal';
      }

      if (config.displayMode === 'popup') {
        config.width = config.width || getDefaultWidth();
        config.height = config.height || 460;
      } else if (config.displayMode === 'modal') {
        config.width = config.width || getDefaultWidth();
      }

      config.accessToken = loadToken();
    }

    function setRequestTimeout(time) {
      REQUEST_TIMEOUT = time;
    }

    function getDefaultWidth() {
      var screenWidth = window.innerWidth || document.documentElement.clientWidth || window.screen.width;
      return Math.min(screenWidth, 400);
    }

    function loginByToken(accessToken) {
      var user = jwtDecode(accessToken);
      setToken({
        user: user,
        token: accessToken
      });
    }

    function logout(callback, source, skipCallbacks) {
      isNewUser = false;
      removeAllTokens();
      util.setAccessTokenListCookie({ value: null }, null, true);
      removeTokenInAuthCookie().then(function () {
        if (!skipCallbacks) {
          _fireLoggedOut(source);
        }
        callback && callback();
      });
    }

    function messageReceiver(iframe) {
      return onMessageReceive;

      function onMessageReceive(message) {

        var trackingId = iframe.config.trackingId;
        var formName = iframe.config.formName;
        switch (message.event) {
          case 'loaded':
            if (iframe.config.displayMode === 'modal') {
              // containerUtils.resizeIframe(iframe, message.params);
              containerUtils.checkBackdropAndContainer(iframe, true);
            }
            iframe.config.loadedCallback && iframe.config.loadedCallback(iframe);
            break;
          case 'stateReady':
            iframe.config.readyCallback && iframe.config.readyCallback(iframe);
            break;
          case 'resize':
            containerUtils.resizeIframe(iframe, message.params);
            break;
          case 'restoreSize':
            if (iframe.config.displayMode === 'inline') {
              containerUtils.resizeIframe(iframe, {
                width: iframe.container.width()
              });
            }
            break;
          case 'loginSuccess':
            onRegistrationOrLoginSuccess(message, function () {
              iframe.config.callback && iframe.config.callback();
              tp.user.refreshAccessToken(true);
              postmessage.broadcast('pianoIdLoginSuccess', message.params);
            });
            break;
          case 'loginFailed':
            _fireCallbacks('loginFailed', message.params);
            break;
          case 'registrationSuccess':
            onRegistrationOrLoginSuccess(message, function () {
              _fireCallbacks('registrationSuccess');
              postmessage.broadcast('pianoIdLoginSuccess', message.params);
            });
            break;
          case 'registrationFailed':
            _fireCallbacks('registrationFailed', message.params);
            break;
          case 'lang':
            updateLang(message.params.lang, true);
            break;
          case 'customEvent':
            _fireCustomEvent(iframe.config, message.params);
            break;
          case 'closed':
            closeIframe(iframe, true);
            iframe.config.closedCallback && iframe.config.closedCallback(iframe);
            break;
          case 'cfFormSubmission':
            log.__protected__.logCustomFormSubmission(trackingId, formName, SOURCE_CUSTOM_FORM);
            break;
          case 'formSend':
          case 'formSkip':
            closeIframe(iframe);
            break;
          case 'cfAppImpression':
            log.__protected__.logCustomFormImpression(trackingId, formName, SOURCE_REGISTRATION);
            break;
          case 'cfAppSubmission':
            log.__protected__.logCustomFormSubmission(trackingId, formName, SOURCE_REGISTRATION);
            break;
          case 'cfFormImpression':
            log.__protected__.logCustomFormImpression(trackingId, formName, SOURCE_CUSTOM_FORM);
            break;
          case 'changeScreen':
            _fireChangeScreen(message.params.screen);
            break;
          case 'screenReady':
            _fireCallbacks('onShow');
            break;
          case 'layoutReady':
            _fireCallbacks('layoutReady', message.params);
            break;
          case 'confirmationEmailSent':
            if (iframe.config.displayMode !== 'inline') {
              closeIframe(iframe);
            }
            break;
          case 'verificationCodeStartCheckout':
            var startCheckoutParams = {};
            try {
              startCheckoutParams = JSON.parse(message.params.startCheckoutParams);
            } catch (e) {
              util.log('error parse start checkout params');
              startCheckoutParams = {};
            }
            closeIframe(iframe, true);

            authByCookie(true, true);
            util.waitCall('refreshUserToken', function () {
              tp.offer.show(startCheckoutParams);
            });
            break;
          case 'removeParamFromGlobalConfig':
            removeParamFromGlobalConfig(message);
            break;
          case 'verifyCode':
            verifyCode(message, iframe);
            break;
          case 'openPasswordlessLoginWithCreatingUser':
            openPasswordlessLoginWithCreateUser();
            break;
          case 'fireCustomEvent':
            _fireCustomEvents(message.params.name);
            break;
          case 'passwordRestored':
            util.removeQueryParamByName('reset_token');
            globalConfig.resetPasswordToken = undefined;
            break;
          case 'emitGAEvent':
            gaService.emitEvent(message.params);
            break;
          case 'redirect':
            document.location.href = message.params.url;
            break;
          default:
            util.debug('Received invalid event type: ' + message.event);
            break;
        }
      }
    }

    function openPasswordlessLoginWithCreateUser() {
      show({ createPasswordlessUser: true });
    }

    function removeParamFromGlobalConfig(message) {
      var parameter = message.params.name;
      camelCaseName = util.fromUnderscoreToCamelCase(parameter);
      delete globalConfig[camelCaseName];

      if (message.params.removeFromUrl) {
        util.removeQueryParamByName(parameter);
      }
    }

    function verifyCode(message, iframe) {
      var verificationCode = message.params.verificationCode;
      var locale = message.params.locale;
      var clientId = globalConfig.aid || tp.aid;
      var access_token = getToken();
      var site = document.location.origin || document.location.href;

      $.ajax({
        url: getPianoIdUrl() + 'api/v1/identity/verification/code',
        dataType: 'jsonp',
        data: {
          client_id: clientId,
          access_token: access_token,
          site: site,
          verification_code: verificationCode,
          lang: locale
        },
        success: function (resp) {
          if (!resp.error) {
            onVerificationSuccess(resp, iframe);
          }
        },
        xhrFields: {
          withCredentials: true
        },
        crossDomain: true
      });
    }

    function onVerificationSuccess(resp, iframe) {
      if (!resp.error && resp.access_token) {
        storeToken({
          token: resp.access_token,
          user: jwtDecode(resp.access_token),
          cookie_domain: resp.site_cookie_domain
        });
      }
      postmessage.send('verificationCodeComplete', resp, iframe.iframeId);
    }

    function onRegistrationOrLoginSuccess(message, callback) {
      if (message && message.params && !message.params.areCookiesSet) {
        message.params.source = PIANO_ID_SOURCE;
        isNewUser = !!message.params.registration;
        setToken(message.params).then(callback);
      } else if (typeof callback === 'function') {
        callback();
      }
    }

    function offerMessageReceiver(message) {
      switch (message.params.event) {
        case 'cfAppImpression':
        case 'cfFormImpression':
          log.__protected__.logCustomFormImpression(message.params.trackingId, message.params.trackingId, SOURCE_CHECKOUT);
          break;
        case 'cfFormSubmission':
          log.__protected__.logCustomFormSubmission(message.params.trackingId, message.params.trackingId, SOURCE_CHECKOUT);
          break;
        case 'changeScreen':
          _fireChangeScreen(message.params.params.screen, true);
          break;
        case 'loginFailed':
        case 'registrationFailed':
        case 'registrationSuccess':
          _fireCallbacks(message.params.event, message.params.params, true);
          break;
      case 'emitGAEvent':
        gaService.emitEvent(message.params.params);
        break;
        default:
          util.debug('Received invalid event type: ' + message.params.event);
          return;
      }
    }

    function getPianoIdDomain() {
      var isSandbox = tp.sandbox || tp.isNonProdEndpoint();
      var domain = globalConfig.iframeUrl || tp.pianoIdUrl || !isSandbox && DEFAULT_PIANOID_ORIGIN || '/';
      if (domain === '/') {
        domain = tp.getEndpointRoot(true);
      }
      return domain.replace(/\/$/, '');
    }

    function getPianoIdUrl() {
      var domain = getPianoIdDomain();
      return domain + '/id/';
    }

    function getIndexForPianoId2IfNeed() {
      return isNewPianoIdVersion() ? 'index3.html' : '';
    }

    function isNewPianoIdVersion() {
      var currentAid = globalConfig.aid || tp.aid;
      return aidsForNewPianoIdVersion.indexOf(currentAid) >= 0;
    }

    function buildIframeUrl(base, config, params, origin) {
      if (!base.match('proxy.html')) {
        base += getIndexForPianoId2IfNeed();
      }

      var query = {
        client_id: config.aid,
        sender: params.iframeId,
        origin: origin || window.location.origin || window.location.href,
        site: window.location.origin || window.location.href,
        parent_uri: window.location.href,
        display_mode: config.displayMode
      };

      var lang = getLang();
      if (lang) {
        query.lang = lang;
      }
      if (!!gaService.getClientId()) {
        query.ga_client_id = gaService.getClientId();
      }
      if (!!gaService.getGAAccount()) {
