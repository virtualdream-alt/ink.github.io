          onload: dfd.resolve,
          widget_init_on_event: {
            'rec': true,
            'opt-in': false
          },
          onclick: function (data) {
            recWidget.fireWidgetCallback('click', data.widgetId, data);
          },
          onwidgetinit: function (data) {
            recWidget.fireWidgetCallback('init', data.widgetId, data);
          },
          onrecsloaded: function (data) {
            recWidget.fireWidgetCallback('recsloaded', data.widgetId, data);
          }
        });

        window.PianoESPConfig = localPianoESPConfig;

        util.debug('pianoESP: loading PianoESP with config:', window.PianoESPConfig);

        recWidgetService.__protected__.insertWidgetLib({
          id: 'pnesplucidsdksel',
          src: _getRootEndpoint() + '/public/sdk/v04/sdk.js?v=' + (localStorage && localStorage.lucidsdkver || 'xxx'),
          onerror: dfd.reject
        });
      } else {
        dfd.reject();
      }

      initPromise = dfd.promise();

      return initPromise;
    };

    var _setVisitorId = function (visitorId) {
      window.PianoESPConfig = $.extend((window.PianoESPConfig || {}), {
        visitor: visitorId
      });
    };

    var _getRootEndpoint = function () {
      var rootEndpoint = '//api-esp.piano.io';

      if (tp.espEndpoint) {
        rootEndpoint = tp.espEndpoint;
      } else if (tp.isSandbox()) {
        rootEndpoint = '//sandbox-api-esp.piano.io';
      }

      return rootEndpoint;
    };

