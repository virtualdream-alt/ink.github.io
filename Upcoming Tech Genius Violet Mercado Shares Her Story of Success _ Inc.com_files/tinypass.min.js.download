tp.define('fbpixel',['jq', 'util'], function ($, util) {

  var isInitialized = false;

  var _init = function () {
    if (isInitialized) {
      return;
    }

    var fbPixelId = tp.getFbPixelId();
    if (!fbPixelId) {
      util.error('fbpixel: Facebook Pixel ID is not defined');
      return;
    }

    util.log('fbpixel: Initialize Facebook Pixel tracking', fbPixelId);

    // Facebook Pixel Code
    !function (f, b, e, v, n, t, s) {
      if (f.fbq) return;
      n = f.fbq = function () {
        n.callMethod ?
          n.callMethod.apply(n, arguments) : n.queue.push(arguments)
      };
      if (!f._fbq) f._fbq = n;
      n.push = n;
      n.loaded = !0;
      n.version = '2.0';
      n.queue = [];
      t = b.createElement(e);
      t.async = !0;
      t.src = v;
      s = b.getElementsByTagName(e)[0];
      s.parentNode.insertBefore(t, s)
    }(window,
      document, 'script', 'https://connect.facebook.net/en_US/fbevents.js');
    // End Facebook Pixel Code

    fbq('set', 'autoConfig', 'false', fbPixelId);
    fbq('init', fbPixelId);

    isInitialized = true;
  };

  var _doTrackEvent = function (trackMethod, eventName, customData) {
    if (!isInitialized) {
      util.log('fbpixel: Skip tracking event - Facebook Pixel is not initalized', eventName, customData);
      return;
    }

    if (!customData) {
      customData = {};
    }

    util.log('fbpixel: Track Facebook Pixel event', eventName, customData);

    fbq(trackMethod, eventName, customData);
  };

  var _trackEvent = function (eventName, customData) {
    _doTrackEvent('track', eventName, customData);
  };

  var _trackCustomEvent = function (eventName, customData) {
    _doTrackEvent('trackCustom', eventName, customData);
  };

  var _isInitialized = function () {
    return isInitialized;
  };

  return {
    name: 'fbpixel',
    isInitialized: _isInitialized,
    __private__: {
      isInitialized: _isInitialized
    },
    __protected__: {
      init: _init
    },
    trackEvent: _trackEvent,
    trackCustomEvent: _trackCustomEvent
  }
