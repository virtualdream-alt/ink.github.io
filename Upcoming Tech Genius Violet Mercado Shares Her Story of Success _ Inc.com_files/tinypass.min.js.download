tp.define('stripeApplePayIntegrationService', ['postmessage'], function (postmessage) {

  var postMessageParams = {};
  var loadScriptPromise = null;
  var stripe = null;
  var paymentRequest = null;
  var canMakePayment = false;
  var termId = '';

  window.addEventListener('message', receiveMessage, false);

  function loadDynamicScript() {
    if (loadScriptPromise != null) {
      return loadScriptPromise;
    }
    var existingScript = document.getElementById('stripe.js');
    if (!existingScript) {
      var script = document.createElement('script');
      script.src = 'https://js.stripe.com/v3/';
      script.id = 'stripe.js';
      document.body.appendChild(script);
      loadScriptPromise = new Promise(function (resolve) {
        script.onload = function() {
          resolve();
        };
      });
      return loadScriptPromise;
    }
  }

  function receiveMessage(event) {
    try {
      postMessageParams = JSON.parse(event.data);
    } catch (x) {
    }

    switch (postMessageParams.event) {
      case 'stripe-applepay-initialized':
        loadDynamicScript();
        break;
      case 'stripe-applepay-initiate-purchase':
        var savedParams = postMessageParams;
        loadDynamicScript()
          .then(function () {
            var apiKey = savedParams.params.apiKey;
            if (stripe === null) {
              stripe = Stripe(apiKey);
            }
            var paymentRequestPayload = savedParams.params.paymentRequestPayload;
            return stripe.paymentRequest(paymentRequestPayload);
          })
          .then(function (request) {
            paymentRequest = request;
            return request.canMakePayment();
          })
          .then(function (payload) {
            canMakePayment = payload && payload.applePay;

            postmessage.broadcast('stripe-applepay-initiate-purchase-complete', {
              canMakePayment: canMakePayment
            });

            if (!canMakePayment) {
              return;
            }

            paymentRequest.on('cancel', function () {
              postmessage.broadcast('stripe-applepay-start-purchase-complete' + termId, {
                status: 'fail'
              });
            });

            paymentRequest.on('paymentmethod', function (ev) {
              var address = ev.paymentMethod.billing_details.address;
              var billingAddress = {
                countryCode: address.country,
                zipCode: address.postal_code,
                billingZipCode: address.postal_code,
              };

              var paymentMethodId = ev.paymentMethod.id;
              postmessage.broadcast('stripe-applepay-start-purchase-complete' + termId, {
                status: 'success',
                paymentMethodId: paymentMethodId,
                payerEmail: ev.payerEmail,
                billingAddress: billingAddress
              });

              ev.complete('success');
            });
          })
          .catch(function (error) {
            console.error(error);
            postmessage.broadcast('stripe-applepay-initiate-purchase-complete', {
              canMakePayment: false
            });
          });
        break;
      case 'stripe-applepay-start-purchase':
        termId = postMessageParams.params.termId || '';
        var paymentRequestPayload = postMessageParams.params.paymentRequestPayload;
        paymentRequest.update(paymentRequestPayload);
        paymentRequest.show();
        break;
      case 'stripe-applepay-abort-request':
        paymentRequest && paymentRequest.abort();
        postmessage.broadcast('stripe-applepay-reopen-request');
        break;
    }

    return {
      name: 'stripeApplePayIntegrationService'
    };
  }
});

tp.define("stripeApplePayIntegrationService", function(){});

