          util.debug('can\'t save mailing lists to storage', e);
        }
    };

    var loadCompleteMailingLists = function () {
      var data = [];

      try {
         data = JSON.parse(localStorage.getItem(MAILING_LIST_KEY));

         if (!Array.isArray(data)) {
           throw new Error('MAINLING_LIST should be array');
         }
      } catch (e) {
        util.debug('can\'t load mailing lists from storage', e);
      }

      return data || [];
    };

    var showNewsletterSignup = function (config) {
        if (!config || !config.mailingListIds) {
          util.log('mailingListIds should be specified');
          return;
        }

        var storedMailingLists = loadCompleteMailingLists();
        var IfFilled = storedMailingLists.indexOf(config.mailingListIds) > -1;

        if (config.notShowIfFilled && IfFilled) {
