    var EXTERNAL_LINK = 'EXTERNAL_LINK';
    var EXTERNAL_EVENT = 'EXTERNAL_EVENT';
    var recWidget = recWidgetService.__protected__.getInstance();
    var USER_PROVIDERS_INIT_TIMEOUT = '300';
    var NOTIFICATION_STATUS = {
