    window.SWG.push(function (subscriptions) {
      subscriptions.setOnPaymentResponse(function (subscriptionPromise) {
        if (onPaymentResponseTriggered) {
          return;
        }

        util.debug('swg: onPaymentResponse is triggered');
        onPaymentResponseTriggered = true;

        subscriptionPromise.then(function (response) {
          util.debug('swg: onPaymentResponse', response);

          completeSubscribeResponse(response, response.purchaseData, response.userData, swgTermId, SwgUiFlows.BUY);
