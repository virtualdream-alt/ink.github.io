      '  tp.gaService.setClientId(ga.getByName(\'' + TRACKER_NAME + '\').get(\'clientId\'));' +
      '  tp.gaService.trackerAppended();' +
      '});';

    return script;
  }

  return me;
});

tp.define('pianoEsp',['jq', 'util', 'log', 'recWidgetService', 'user', 'api'],
  function ($, util, log, recWidgetService, user, api) {
