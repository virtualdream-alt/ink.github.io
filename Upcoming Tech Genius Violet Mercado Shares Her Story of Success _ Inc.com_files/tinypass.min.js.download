tp.define('observer',['util', 'jq'], function (util, $) {
  

  var MutationObserver = window.MutationObserver ||
    window.WebKitMutationObserver ||
    window.MozMutationObserver;

  var isMutationObseverSupport = !!MutationObserver;


  var attrsConfig = {
    attributes: true
  };

  function AttrObserver(callback) {
    this.callback = callback;
    this._isActive = false;
    this.targets = [];
    this.timeout = 200;
  }

  AttrObserver.prototype._mutationSearch = function () {
    var that = this;
    this.targets.forEach(function (target) {
      var state = that._getState(target.element);
      if (!state === target.currentState) {
        target.currentState = state;
        that.callback([{
          target: target.element
        }]);
      }
    });
    if (this._isActive) {
      setTimeout(this._mutationSearch.bind(this), this.timeout);
    }
  };

  AttrObserver.prototype._getState = function (element) {
    var attributes = [].slice.call(element.attributes);
    var attributesMap = attributes.reduce(function (acc, attr) {
      acc[attr.name] = element.getAttribute(attr.name);
      return acc;
    }, {});

    return JSON.stringify(attributesMap);
  };

  AttrObserver.prototype.observe = function (element) {
    this.targets.push({
      id: util.randomString(),
      element: element,
      currentState: this._getState(element)
    });
    if (!this._isActive) {
      this._isActive = true;
      this._mutationSearch();
    }
  };

  AttrObserver.prototype.disconnect = function () {
    this._isActive = false;
    delete this.targets;
  };

  return {
    name: 'observer',
    create: function ($element, callback) {
      if (!$element.length) {
        util.error('element must be NODE');
      }
      var Observer = isMutationObseverSupport ? MutationObserver : AttrObserver;
      var observer = new Observer(function (mutations) {
        var elements = mutations.map(function (mutation) {
          return $(mutation.target);
        });
        return typeof callback === 'function' && callback(elements);
      });

      observer.observe($element[0], attrsConfig);

      return observer;
    }
  };

});

