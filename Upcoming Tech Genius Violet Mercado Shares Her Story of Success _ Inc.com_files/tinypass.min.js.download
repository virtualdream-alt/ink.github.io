tp.define('applePayIntegrationService', ['jq', 'postmessage', 'util'], function ($, postmessage, util) {

  // errorCode = 110 - Payment completion error
  // errorCode = 111 - Merchant validation error
  // errorCode = 112 - Error occurred while starting Apple Pay session
  // errorCode = 113 - Can not receive payment session from server

  var applePayJsApiVersion = 3;
  var postMessageParams = {};
  var resourceName;
  var session;
  var canMakePayments = false;
  var termId;

  function receiveMessage(event) {
    try {
      postMessageParams = JSON.parse(event.data);
    } catch (x) {

    }

    switch (postMessageParams.event) {
      case 'provider-component-initiated': {
        if (canMakePayments) {
          postmessage.broadcast('apple-pay-init', {});
        }
        break;
      }
      case 'initPwApplePay':
      case 'startCheckout':
        canMakePayments = util.__protected__.isApplePayAllowed();
        postmessage.broadcast('EVENT_APPLE_PAY_CAN_MAKE_PAYMENTS', { canMakePayments: canMakePayments });
        break;
      case 'pay-session-received': {
        try {
          session.completeMerchantValidation(postMessageParams.params.applePaySession);
        } catch (e) {
          postmessage.broadcast('payment-error', { errorCode: 111, errors: e });
        }
        break;
      }
      case 'apple-pay-complete-payment': {
        if (!isTermIdEqual(postMessageParams.params.termId)) {
          return;
        }

        session.completePayment(postMessageParams.params.status, postMessageParams.params.errors);
        break;
      }
      case 'start-purchase': {
        termId = postMessageParams.params.termId || '';
        resourceName = postMessageParams.params.resourceName;
        session = new ApplePaySession(applePayJsApiVersion, createRequest());

        session.onvalidatemerchant = function (event) {
          validateMerchantAndRequestPaymentSession(event.validationURL, resourceName);
        };

        session.onpaymentauthorized = function (event) {
          try {
            postmessage.broadcast('apple-pay-payment-authorized', {
              paymentData: event.payment,
              termId: termId
            });
          } catch (e) {
            postmessage.broadcast('payment-error', { errorCode: 110, error: e });
          }
        };

        try {
          session.begin();
        } catch (e) {
          postmessage.broadcast('payment-error', { errorCode: 112, error: e, termId: termId });
        }
        break;
      }
      case 'abort-apple-pay-session' : {
        abortSession();
      }
    }

    function isTermIdEqual(idOfTerm) {
      return termId ? termId === idOfTerm : true;
    }

    function createRequest() {
      var request = {
        countryCode: postMessageParams.params.countryCode,
        currencyCode: postMessageParams.params.currencyCode,
        supportedNetworks: ['visa', 'masterCard', 'amex', 'discover'],
        merchantCapabilities: ['supports3DS'],
        requiredShippingContactFields: postMessageParams.params.requiredShippingContactFields || [],
        requiredBillingContactFields: postMessageParams.params.requiredBillingContactFields || [],
        total: { label: postMessageParams.params.resourceName, amount: postMessageParams.params.chargeAmount },
      };
      return request;
    }

    function abortSession() {
      try {
        session.abort();
      } catch (e) {
        console.error(e);
      }
    }

    function validateMerchantAndRequestPaymentSession(validationUrl, displayedCaption) {
      var request = {
        validationUrl: validationUrl,
        displayedCaption: displayedCaption,
        userToken: null,
        userProvider: null,
        userRef: null,
        janrainCaptureToken: null,
        domainName: window.location.hostname,
        aid: null,
        termId: termId
      };

      postmessage.broadcast('apple-pay-pay-session-request', request);
    }
  }

  function initialize() {
    window.addEventListener('message', receiveMessage, false);
  }

  function applePayCanMakePaymentsWithActiveCard () {
    var rejectResult = {
      then: function (callbackFn) {
        return callbackFn(null);
      },
      catch: function () {
      }
    };
    if (!tp.applePayMerchantId) {
      return rejectResult;
    }

    var ifCanMakePayments = util.__protected__.isApplePayAllowed();

    if (ifCanMakePayments && tp.applePayMerchantId) {
      return ApplePaySession.canMakePaymentsWithActiveCard(tp.applePayMerchantId);
    } else {
      return rejectResult;
    }
  }

  return {
    name: 'applePayIntegrationService',
    initialize: initialize,
    __protected__: {
      applePayCanMakePaymentsWithActiveCard: applePayCanMakePaymentsWithActiveCard
    }
  }
});

