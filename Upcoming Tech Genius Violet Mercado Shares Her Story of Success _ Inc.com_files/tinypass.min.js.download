    }

    if (!source) {
      util.log('Parameter source is required to log impression.');
    }

    var data = {
      aid: tp.aid,
      pageview_id: tp.main.fetchPageViewId(),
      custom_form_name: formName,
      custom_form_source: source
    };

    var token = tp.user.getProvider().getToken();

    if (token) {
      data.user_token = token;
    }

    if (trackingId) {
      data.tracking_id = trackingId;
    }
