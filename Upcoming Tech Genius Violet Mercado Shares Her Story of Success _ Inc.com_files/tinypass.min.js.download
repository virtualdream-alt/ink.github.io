  }

  function setUpPianoIdCallbacks() {
    if (!isAddedUserProviderCallbacks) {
      tp.pianoId.setCallbacks({
        loggedIn: function (data) {
          util.log('offer.js - piano id user logged in');
          var params = {
            user_token: data.token,
            userToken: data.token,
            user: data.user,
            reloadAfterLogin: true,
            termId: loginRequiredTermId
          };

          if (loginRequiredIframeId) {
            var iframe = containerUtils.getIframe(loginRequiredIframeId);

            if (iframe) {
              postmessage.send('externalLoginSuccess', params, loginRequiredIframeId);
              _reload(loginRequiredIframeId, params);
              delete params.termId;
              _reloadAll(params, loginRequiredIframeId);
            } else {
              _reloadAll(params);
            }
          } else {
            _reloadAll(params);
          }
        },
        loggedOut: function (data) {
          if (data.source !== LOGIN_LOGOUT_SOURCE) {
            var config = {};
            config.userProvider = user.getProvider().getName();
            config.userToken = user.getProvider().getToken() && user.getProvider().getToken().length > 0 ? user.getProvider().getToken() : "";
            config.noNeedStartCheckout = true;
            _reloadAll(config);
          }
          loginRequiredTermId = null;
          loginRequiredIframeId = null;
        }
      });
      isAddedUserProviderCallbacks = true;
    }
  }

  function _applyPreloadIframe(iframe) {
    return containerUtils.showPreloadedContainer(iframe);
  }

  var _fireCallback = function (config) {
    var event = config.offerId === containerUtils.fakeOfferId ? 'showTemplate' : 'showOffer';
    if (eventManager.hasCallback(event)) {
      eventManager.fireCallbacks(event, config, null, true);
    }
  };

  /**
   * Shows an offer.
   *
   * Look at tp.offer.show for documentation for the configuration
   *
   * @param config
   * @private
   */
  var _show = function (config) {
    if (!config.offerId && config.preview !== true) {
      util.error('Must specify offerId');
      return;
    }

    if (_isLockedConfig(config)) {
      return false;
    }

    if (!config.preview) {
      _lockConfig(config);
    }

    setTimeout(function () {
      _releaseConfig(config);
    }, lockTimeout);

    util.waitCall('refreshUserToken', function () {
      // util.waitCall("pageTrack", function () {
      util.waitCall('refreshAccessTokenList', function () {
        util.waitCall('myAccountLogin', function () {
          util.startCall('offerCreate');

          var createIframeFn = config.createIframeFn ?
            config.createIframeFn : _createOfferIframe;
          var iframe = createIframeFn(config);
          if (!iframe) {
            util.doneCall('offerCreate');
            util.error('Could not initialize offer; check configuration');
            return;
          }

          if (_isDirectCheckout(config)) {
            _startDirectCheckout(iframe.iframeId);
          }

          _fireCallback(config);
        });
      });
      // });
    });
  };

  var _preload = function (config) {
    var dfd = $.Deferred();
    var promise = dfd.promise();
    var iframeLoad = $.Deferred();

    var obj = {
      promise: promise,
      show: function () {
        promise
          .pipe(_applyPreloadIframe)
          .pipe(iframeLoad.promise)
          .then(function (iframe) {
            trackStat(iframe);

            if (_isDirectCheckout(config)) {
              _startDirectCheckout(iframe.iframeId);
            }

            _fireCallback(config);
          }, function () {
            delete config.isPreload;
            _show(config);
          });
      }
    };

    if (config.displayMode !== 'popup' && config.offerId) {
      util.waitCall('refreshAccessTokenList', function () {
        config.isPreload = true;

        util.startCall('offerCreate');
        var createIframeFn = config.createIframeFn ? config.createIframeFn : _createOfferIframe;
        var iframe = createIframeFn(config);
        if (!iframe) {
          util.doneCall('offerCreate');
          util.error('Could not initialize offer; check configuration');
          dfd.reject();
        } else {
          iframe.element.load(function () {
            iframeLoad.resolve(iframe);
          });
          dfd.resolve(iframe);
        }
      });
    } else {
      // can't preload popup
      dfd.reject();
    }
    return obj;
  };

  var _isDirectCheckout = function (config) {
    return !!config.directCheckout && config.displayMode !== 'inline';
  };

  var _startDirectCheckout = function (iframeId) {
    util.waitCall('offerCreate', function () {
      postmessage.send('startDirectCheckout', {}, iframeId);
    });
  };

  var _startCheckout = function (config, sender) {
    if (containerUtils.hasModalIframe()) {
      containerUtils.closeModalContainer();
    }
    util.debug('creating checkout: ', config);
    config.displayMode = config.displayMode === 'popup' ? 'popup' : 'modal';
    config.isCheckout = true;
    config.parent = sender;

    util.waitCall('refreshUserToken', function () {
      util.waitCall('refreshAccessTokenList', function () {
        util.startCall('offerCreate');

        var iframe = _createOfferIframe(config);

        containerUtils.__protected__.addChildToInlineIframe(iframe, sender);

        if (!iframe) {
          util.doneCall('offerCreate');
          util.error('Could not start checkout; check configuration');
        } else if (needReloadOnStartCheckout) {

          util.waitCall('offerCreate', function () {
            needReloadOnStartCheckout = false;
            var reloadConfig = {};
            reloadConfig.userProvider = user.getProvider().getName();
            reloadConfig.userToken = user.getProvider().getToken() && user.getProvider().getToken().length > 0 ?
              user.getProvider().getToken() : '';
            reloadConfig.noNeedStartCheckout = true;
            reloadConfig.noNeedStartCheckoutAfterLogin = true;

            _reloadAll(reloadConfig, iframe.iframeId);
          });
        }
      });
    });
  };

  var _startRedeemVoucher = function (config, sender) {
    containerUtils.closeModalContainer();
    if (!config) {
      config = {};
    }
    config.isRedemption = true;
    util.debug('starting redeem voucher: ', config);
    config.displayMode = config.displayMode === 'popup' ? 'popup' : 'modal';
    config.isCheckout = false;
    config.parent = sender;
    util.waitCall('refreshAccessTokenList', function () {
      util.startCall('offerCreate');
      if (!_createOfferIframe(config)) {
        util.doneCall('offerCreate');
        util.error('Could not start redeem; check configuration');
      }
    });
  };

  /**
   * Reload offer with specified iframeId
   * @param iframeId Identifier of offer
   * @param config Piece of new configuration for offer
   * @private
   */
  var _reload = function (iframeId, config) {
    postmessage.send('reloadOffer', config, iframeId);
  };

  /**
   * Reload all offer except one (if specified)
   * @param config Piece of new configuration for offers
   * @param exceptIframeId Identifier of excepted offer
   * @private
   */
  var _reloadAll = function (config, exceptIframeId) {
    postmessage.broadcast('reloadOffer', config, exceptIframeId);
  };

  function mergeLoginRequiredParamsWithConfig(iframe, params) {
    var config = {
      reloadAfterLogin: true
    };

    $.extend(config, iframe.config);

    if (config.singleContainer) {
      delete config.singleContainer;
    }

    if (params.termId) {
      config.termId = params.termId;
    }

    if (params.term) {
      config.term = {};
      config.term = merge(config.term, params.term);
    }

    if (params.startScreen) {
      config.startScreen = params.startScreen;
    }

    if (params.isRedemption) {
      config.isRedemption = params.isRedemption;
      config.voucherCode = params.redemption && params.redemption.form && params.redemption.form.redeemCode;
      config.termId = null;
    }

    if (params && params.currentState && iframe.config.displayMode === 'modal') {
      config.allowReturnToStartState = true;
    }

    if (params.preventExternalLogin) {
      config.preventExternalLogin = params.preventExternalLogin;
    }

    if (params.sharedAccountCode) {
      config.sharedAccountCode = params.sharedAccountCode;
    }

    config.displayMode = 'modal';

    return config;
  }

  var _checkThreeDSRedirect = function() {
    var PROCESS_3DS_RURL = 'pn_rurl_3ds';
    var rurl_3ds = util.getQueryParamByName(PROCESS_3DS_RURL);

    if (rurl_3ds) {
      var additionalParamsNames = {
        offerId: 'pn_offer_id',
        oid: 'pn_oid',
        termId3ds: 'pn_term_id',
        providerId: 'pn_provider_id',
        thirdPartyParams: 'third_party_params'
      };

      var parametersForRemove = [PROCESS_3DS_RURL];
      var additionalParams = Object.keys(additionalParamsNames).reduce(function(acc, paramKey) {
        var nAcc = $.extend({}, acc);
        nAcc[paramKey] = util.getQueryParamByName(additionalParamsNames[paramKey]);
        parametersForRemove.push(additionalParamsNames[paramKey]);
        return nAcc;
      }, {});

      util.removeQueryParamByName(parametersForRemove);

      tp.restoreCustomVaribalesFromLocalstorage();

      _show($.extend({
          displayMode: 'modal',
          rurl_3ds: rurl_3ds
        },
        additionalParams
      ));
    }
  };

  return {
    name: 'offer',
    /**
     * Shows an offer on the page.
     *
     * @param config
     * The parameters that can get specified are:
     *
     *     offerId (required):
     *         The id of the offer being loaded, found in the publisher dashboard
     *
     *     templateId (required):
     *         The id of the template being loaded, found in the publisher dashboard
     *
     *     displayMode (optional; default=modal):
     *         Configures the display mode of the offer/template. Valid options
     *         are: inline, modal. Inline requires that a containerSelector is specified.
     *
     *         When set to modal, the semi-transparent backdrop will appear and the
     *         offer and template will appear in an iframe in the middle of the browser.
     *
     *         When set to inline, the iframe will get loaded in the
     *
     *     containerSelector (conditionally required):
     *         The jQuery selector that targets a single DOM element on the page that the
     *         offer and template should appear in. For example a containerSelector of
     *         ".tp-offer-form" should be used if there were one, and only one:
     *
     *             <div class="tp-offer-form"></div>
     *
     *         that resided on the page that the offer+template should appear in.
     *
     *         This option is only used when displayMode is inline, and must be a valid
     *         selector for a single DOM element on the page.
     *
     *     close (optional):
     *         A callback that fires when the user dismisses a modal. This event only fires
     *         when the user dismisses an iframe that is in modal mode.
     *
     *     cancel (optional):
     *         A callback that fires when the user cancels a transaction.
     *
     *     complete (optional):
     *         A callback that fires when the user completes a transaction.
     *
     *     customEvent (optional):
     *         A callback that fires when the user performs an action in the template that
     *         the publisher has configured to listen for.
     *
     *         For example, a publisher might implement code on their website such as:
     *
     *         tp.offer.show({
     *             // all required fields, plus
     *             customEvent : function(event) {
     *                 switch(event.params.eventName) {
     *                     case "login":
     *                         window.location.href = "/login";
     *                         break;
     *                 }
     *             }
     *         });
     *
     *         In their template, their markup would contain:
     *
     *             <a href="#" ng-click="customEvent('login')">Login</a>
     *
     *         This callback would get fired when the user clicked Login from within template.
     *
     */
    show: function (config) {
      if (config.displayMode === 'inline') {
        containerUtils.operationWithContainer(config.containerSelector, function () {
          _show(config);
        });
      } else {
        _show(config);
      }
    },
    deferredShow: function (config) {
      return _preload(config);
    },
    close: function () {
      containerUtils.closeModalContainer();
    },
    closeInline: function (selector) {
      containerUtils.__protected__.closeInlineContainers(selector);
    },
    startCheckout: function (config) {
      _startCheckout(config);
    },
    startRedeemVoucher: function (config) {
      _startRedeemVoucher(config);
    },
    createOfferIframe: function (config) {
      return _createOfferIframe(config);
    },
    showBackdrop: function () {
      containerUtils.showBackdrop();
    },
    hideBackdrop: function () {
      containerUtils.hideBackdrop();
    },
    centerBackdrop: function (container) {
      containerUtils.centerBackdrop(container);
    },
    registerCallback: function (callbackName, callback) {
      eventManager.addEvent(callbackName);
