          type: 'post',
          url: queryString,
          async: false,
          xhrFields: {
            withCredentials: true
          }
        };
      }

      ajaxSettings.error = function () {
        util.debug('Beacon request has been failed', arguments);
      };

      $.ajax(ajaxSettings);
    }
  };

  return {
    name: 'log',
    /**
     * Send request about micro conversion
     * All this function have duplicate in piano object
     */
    logMicroConversion: _logMicroConversion,
    /**
     * Send request about conversion
