tp.define('interaction', ['jq', 'util'], function ($, util) {

  var me = {
    __private__: {},
    __protected__: {}
  };

  me.name = 'interaction';
  me.bind = bind;
  me.unbindAll = unbindAll;

  me.__private__.boundInteractions = [];

  function bind(callback, interactions, consumeEventFn) {
    var actionsToJqueryEvents = function (actions) {
      var actionToEvents = {
        'click': 'click',
        'doubleClick': 'dblclick',
        'rightClick': 'contextmenu',
        'mouseOut': 'mouseout',
        'mouseIn': 'mouseenter'
      };
      var events = [];
      for (var i = 0; i < actions.length; i++) {
        var action = actions[i];
        events.push(actionToEvents[action]);
      }
      return events;
    };

    if (typeof consumeEventFn !== 'function') {
      consumeEventFn = function () {
        return true;
      }
    }

    util.__protected__.documentReady(function () {
      for (var i = 0; i < interactions.length; i++) {
        var interaction = interactions[i];
        var actionsString = actionsToJqueryEvents(interaction.elementActions).join(' ');
        var elementSelector = interaction.elementSelector;
        var isRepetitive = interaction.repetitive;
        if (actionsString.trim().length) {
          (function (selector, action, isRepetitive) {
            var interactionEventHandler = function () {
              if (!consumeEventFn()) {
                return;
              }

              if (!isRepetitive) {
                $('body').off(action, selector, interactionEventHandler);
              }
              callback();
            };

            $('body').on(action, selector, interactionEventHandler);
            me.__private__.boundInteractions.push({
              action: actionsString,
              selector: elementSelector,
              func: interactionEventHandler
            });
          })(elementSelector, actionsString, isRepetitive);
        }
      }
    });
  }

  function unbindAll() {
    $.each(me.__private__.boundInteractions, function (index, value) {
      $('body').off(value.action, value.selector, value.func);
    });

    me.__private__.boundInteractions = [];
  }

  return me;
});

