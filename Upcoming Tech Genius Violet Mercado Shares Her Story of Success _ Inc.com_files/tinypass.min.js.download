  };

  var _logCustomFormSubmission = function (trackingId, formName, source) {
    if (!formName) {
      util.log('Parameter formName is required to log submission.');
    }

    if (!source) {
      util.log('Parameter source is required to log submission.');
    }
    var data = {
      aid: tp.aid,
      pageview_id: tp.main.fetchPageViewId(),
      custom_form_name: formName,
