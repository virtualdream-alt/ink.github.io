  var applePaySrc = 'https://js.braintreegateway.com/web/3.22.2/js/apple-pay.min.js';

  var session;
  var btApplePayInstance;
  var paymentRequest;
  var isBillingAddressShown;
  var clientToken;
  var termId = '';

  window.addEventListener('message', receiveMessage, false);

  function receiveMessage(event) {
    try {
      postMessageParams = JSON.parse(event.data);
    } catch (x) {

    }

    if (['loaded', 'startCheckout'].indexOf(postMessageParams.event) !== -1) {
      var canMakePayments = util.__protected__.isApplePayAllowed();
      postmessage.broadcast('EVENT_APPLE_PAY_CAN_MAKE_PAYMENTS', {canMakePayments: canMakePayments});
    } else if (postMessageParams.clientToken) {
      initPayment(postMessageParams.clientToken);
    } else if (postMessageParams.event === 'applePayCheckoutFlowProperties') {
      isBillingAddressShown = postMessageParams.params.isBillingAddressShown;
    } else if (postMessageParams.event === 'applyTaxFailed') {
      session.abort();
    } else if (postMessageParams.event === 'checkApplePayBtInstance') {
      if (btApplePayInstance) {
        postmessage.broadcast('applePayInit', {});
      }
    } else if (postMessageParams.event === 'applyTaxCompleted') {
        completePayment(postMessageParams.params.nonce);
    } else if (postMessageParams.beginSession) {
      var copiedPostMessageParams = {};

      if (postMessageParams.resourceTitle) {
        copiedPostMessageParams = postMessageParams;
      }

      if (postMessageParams.termId) {
        termId = postMessageParams.termId;
      }

      var paymentRequestParams = {
        currencyCode: postMessageParams.currencyCode,
        total: {
          label: postMessageParams.resourceTitle,
          amount: postMessageParams.chargeAmount
        }
      };

      if (isBillingAddressShown) {
        paymentRequestParams.requiredShippingContactFields = ['email'];
        paymentRequestParams.requiredBillingContactFields = ['postalAddress'];
      }

      try {
        paymentRequest = btApplePayInstance.createPaymentRequest(paymentRequestParams);
      } catch (msg) {
        postmessage.broadcast('payment-error', {errorCode: 101});
      }

      session = new ApplePaySession(applePayJsApiVersion, paymentRequest);

      session.onvalidatemerchant = function (event) {
        btApplePayInstance.performValidation({
          validationURL: event.validationURL,
          displayName: postMessageParams.resourceTitle || copiedPostMessageParams.resourceTitle
        }, function (validationErr, merchantSession) {
          if (validationErr) {
            session.abort();
            postmessage.broadcast('payment-error', {errorCode: 102});
            return;
          }
          session.completeMerchantValidation(merchantSession);
        });
      };

      session.onpaymentauthorized = function (event) {
        btApplePayInstance.tokenize({
          token: event.payment.token
        }, function (tokenizeErr, payload) {
          if (tokenizeErr) {
            postmessage.broadcast('payment-error', {errorCode: 103});
            session.completePayment(ApplePaySession.STATUS_FAILURE);
            return;
          }

          var applePayNonceData = {
            nonce: payload.nonce
          };
          if (isBillingAddressShown && event.payment.shippingContact) {
            applePayNonceData.user = {
              email: event.payment.shippingContact.emailAddress
            };

            onPaymentAuthorized(event.payment.billingContact, applePayNonceData);
          } else {
            completePayment(applePayNonceData);
          }
        });
      };

      session.begin();
    }
  }

  function onPaymentAuthorized(billingContact, applePayNonceData) {
    var params = {
      countryCode: billingContact.countryCode ? billingContact.countryCode.toUpperCase() : billingContact.countryCode,
      zipCode: billingContact.postalCode,
      billingZipCode: billingContact.postalCode,
      nonce: applePayNonceData
    };

    postmessage.broadcast('applePayPaymentAuthorized' + termId, params);
  }

  function completePayment(applePayNonceData) {
    session.completePayment(ApplePaySession.STATUS_SUCCESS);
    postmessage.broadcast('applePayNonce' + termId, applePayNonceData);
  }

  function initPayment(token) {
    clientToken = token;
    addClientScript();
  }

  function initBraintree() {
    braintree.client.create({
      authorization: clientToken
    }, function (clientErr, clientInstance) {
      if (clientErr) {
        postmessage.broadcast('payment-error', {errorCode: 100});
        util.log('[TP] Client error: ' + clientErr.message);
        return;
      }

      braintree.applePay.create({
        client: clientInstance
      }, function (applePayErr, applePayInstance) {
        if (applePayErr) {
          postmessage.broadcast('payment-error', {errorCode: 101});
          return;
        }

        btApplePayInstance = applePayInstance;
        postmessage.broadcast('applePayInit', {});
      });
    });
  }

  function addClientScript() {
    if (isClientScriptAdded()) {
      return;
    }

    addScript(clientSrc, function () {
      braintreeClientLoaded = true;
      addApplePayScript();
    });
  }

  function addApplePayScript() {
    if (isApplePayScriptAdded()) {
      return;
    }

    addScript(applePaySrc, function () {
      braintreeApplePayLoaded = true;
      initBraintree();
    });
  }

  function addScript(src, onload) {
    var a = document.createElement('script');
    var b = document.getElementsByTagName('script')[0];
    b.parentNode.insertBefore(a, b);

    a.type = 'text/javascript';
    a.async = true;
    a.onload = onload;
    a.src = src;
  }

  function isClientScriptAdded() {
    var scripts = document.getElementsByTagName('script');
    for (var i = 0, k = scripts.length; i < k; i++) {
      if (scripts[i].src.indexOf(clientSrc) > -1) {
        return true;
      }
    }
  }

  function isApplePayScriptAdded() {
    var scripts = document.getElementsByTagName('script');
    for (var i = 0, k = scripts.length; i < k; i++) {
      if (scripts[i].src.indexOf(applePaySrc) > -1) {
        return true;
      }
    }
  }

  return {
    name: 'applePay'
  };
});

