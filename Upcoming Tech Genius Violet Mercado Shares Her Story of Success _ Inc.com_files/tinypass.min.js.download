              return;
            }
            checkSubscription(entitlementInGoogle).then(function (response) {
              if (onPaymentResponseTriggered) {
                // in case of subscribe flow based on redirects (e.g. in Edge), user will be redirected back to the site
                // after purchase and "onPaymentResponse" callback should be triggered when page is loaded. In this case
                //  we should just complete subscribe flow and should not trigger any other flows.
                util.debug('swg: onPaymentResponse is already triggered. Skipping entitlements flow.');
