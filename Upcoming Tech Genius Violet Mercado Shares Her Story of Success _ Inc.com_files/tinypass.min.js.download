      var dfd = $.Deferred();

      tp.api.callApi('/swg/check/external', {
        entitlements: JSON.stringify(entitlements)
      }, function (rspData) {
        if (rspData.code !== 0) {
          util.log('swg: check external entitlement response - ' + rspData.message);
          dfd.reject();
        }

        var data = rspData.data;
        if (!data){
          util.log('swg: check external entitlement empty response');
          dfd.reject();
        }

        dfd.resolve(data);
      });

      return dfd.promise();
    }

    function completeSubscribeResponse(response, purchaseData, userData, termId, uiFlow) {
      tp.api.callApi('/swg/subscribe', {
        term_id: termId,
        purchase_data: JSON.stringify(purchaseData),
