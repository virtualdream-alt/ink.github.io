tp.define("tinypassErrorHandler", ["postmessage", "api", "jq"], function (postmessage, api, $) {
  var EVENT_TP_ERROR_HANDLER = 'EVENT_TP_ERROR_HANDLER';
  var ANON_LOG_ENDPOINT = '/anon/error/log';
  var RANDOM_SENDER_CHANCE = 10000;
  var SEND_ERRORS_TIMEOUT = 30000;
  var privateErrors = [];
  var apiEndpoint = api.getEndpoint() + ANON_LOG_ENDPOINT;

  function isInArray(errorsArray, errorObject) {
    var strings = errorsArray.map(JSON.stringify);
    return strings.indexOf(JSON.stringify(errorObject)) !== -1;
  }

  function randomInteger(min, max) {
    var rand = min - 0.5 + Math.random() * (max - min + 1);
    rand = Math.round(rand);
    return rand;
  }

  function __addError(error) {
    // do not allow duplicates
    if (isInArray(privateErrors, error)) {
      return;
    }

    privateErrors.push(error);
  }

  function __postmessageErrorHandler(data) {
    var errors = data.params;

    if (!Array.isArray(errors)) {
      return;
    }

    for (var i = 0; i < errors.length; i++) {
        __addError(errors[i]);
    }
  }

  function isNotTinypassError(stack) {
    return stack.indexOf('tinypass') === -1;
  }

  function __errorEventListener(event) {
    var details = {};
    var hasError = (('error' in event) && event.error);
    details.stack = hasError ? event.error.stack : 'n/a';
    details.message = hasError ? event.error.message : 'n/a';
    details.userAgent = navigator.userAgent;

    if (isNotTinypassError(details.stack)) {
      return;
    }
