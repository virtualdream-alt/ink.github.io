        subscriptions.getEntitlements().then(function (entitlements) {
          entitlementsLoaded = true;
          util.debug('swg: getEntitlements', entitlements);

          if (entitlements.enablesThis()) {
            var entitlementInGoogle = entitlements.getEntitlementForSource('google');
            if (!entitlementInGoogle) {
              var externalEntitlements = entitlements.entitlements;
              if (externalEntitlements) {
                checkExternalEntitlement(externalEntitlements).then(function () {
                  util.debug('swg: checkExternalEntitlement');
                  entitlements.ack();
                }, function () {
                  util.log('swg: checkExternalEntitlement failed');
                });
