                    entitlements: entitlements,
                    consent: true
                  }).then(function (response) {
                    util.debug('swg: completeDeferredAccountCreation', response);

                    completeSubscribeResponse(response, response.purchaseData, response.userData, termId,
                      SwgUiFlows.DEFERRED_ACCOUNT_CREATION);
                  }).catch(function (e) {
                    util.log('swg: completeDeferredAccountCreation failed', e);
                  });
                  break;
                default:
                  util.debug('swg: check subscription response type ' + response.type + '. Nothing to do');
              }
            }, function () {
              util.log('swg: checkSubscription failed');
            });

