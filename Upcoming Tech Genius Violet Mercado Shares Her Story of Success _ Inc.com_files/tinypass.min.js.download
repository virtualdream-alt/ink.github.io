      }
    }

    var markupIsInjected = false;
    var _injectMarkup = function (markupData) {
      if (markupIsInjected) {
        return;
      }
      if (markupData) {
        util.debug('swg: rendering swg markup with config:', markupData);

        var markup = '{\n' +
          '   "@context": "http://schema.org",\n' +
          '   "@type": "CreativeWork",\n' +
          '   "isAccessibleForFree": ' + markupData.isAccessibleForFree;
        if (markupData.isAccessibleForFree === false && markupData.productId) {
          markup = markup +
            ',\n' +
            '   "isPartOf": {\n' +
            '       "@type": ["CreativeWork", "Product"],\n' +
            '       "name" : "' + markupData.resourceName + '",\n' +
            '       "productID": "' + markupData.productId + '"\n' +
            '   }';
        }
        markup = markup + '\n}';

        var markupScriptElement = document.createElement('script');
        markupScriptElement.type = 'application/ld+json';
        markupScriptElement.innerHTML = markup;

        document.getElementsByTagName('script')[0].parentNode.appendChild(markupScriptElement);
        markupIsInjected = true;
      }
    };

    var libraryIsAdded = false;
    var _addLibrary = function () {
      if (libraryIsAdded) {
        return;
      }
      util.debug('swg: adding swg.js library');

      var libraryScriptElement = document.createElement('script');
      libraryScriptElement.type = 'text/javascript';
      libraryScriptElement.setAttribute('subscriptions-control', 'manual');
      libraryScriptElement.async = true;
      libraryScriptElement.src = 'https://news.google.com/swg/js/v1/swg.js';

      document.getElementsByTagName('script')[0].parentNode.appendChild(libraryScriptElement);
      libraryIsAdded = true;
    };

    return {
      name: 'swg',
      __protected__: {
        injectMarkup: _injectMarkup,
        addLibrary: _addLibrary,
        initSwgFlow: _initSwgFlow
      },
      registerCallback: function (event, callback) {
        eventManager.addEvent(event);
        eventManager.registerCallback(event, callback);
      }
    };
  });

