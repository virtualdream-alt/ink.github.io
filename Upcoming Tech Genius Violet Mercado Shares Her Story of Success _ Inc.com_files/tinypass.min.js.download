                  var userToken = response.user_token;
                  subscriptions.showLoginPrompt().then(function () {
                    loginByToken(userToken);
                    fireExternalCheckoutCompleteCallback(SwgUiFlows.LOGIN_PROMPT);
                  }).catch(function (e) {
                    util.log('swg: showLoginPrompt failed', e);
                  });
                  break;
