    $.each(propsToCopy, function (index, value) {
      if (c[value]) {
        iframeParams[value] = c[value];
      }

      if (value === 'showCloseButton') {
        if (c[value] === undefined) {
          iframeParams[value] = true;
        } else {
          iframeParams[value] = c[value];
        }
      }
    });

    // override trackingId from query parameter (for loose conversion flow)
    var trackingId = decodeURIComponent(util.getQueryParamByName('_ptid'));
    if (trackingId) {
      iframeParams.trackingId = trackingId;
    }

    if (c.preview) {
      iframeParams.activeMeters = JSON.stringify([{
        meterName: 'preview-meter',
        views: 10,
        viewsLeft: 5,
        maxViews: 20,
        totalViews: 20
      }]);
    }

    return iframeParams;
  }

  function _compactConfig(config) {
    var displayMode = config.displayMode || 'modal';
    return [config.offerId, displayMode, config.containerSelector].join('|');
  }

  function _isLockedConfig(config) {
    var jsonConfig = _compactConfig(config);
    return !!lockedConfigs[jsonConfig];
  }

  function _lockConfig(config) {
    var jsonConfig = _compactConfig(config);
    lockedConfigs[jsonConfig] = true;
  }

  function _releaseConfig(config) {
    var jsonConfig = _compactConfig(config);
    delete lockedConfigs[jsonConfig];
  }

  /**
   * Creates an offer iframe.
   *
   * Look at tp.offer.show for documentation for parameters
   *
   * @param c     The configuration for the offer.
   * @returns {*}
   * @private
   */
  var _createOfferIframe = function (c) {
    closeState = 'close';

    if (typeof (c.displayMode) === 'undefined') {
      c.displayMode = 'modal';
    } else if (c.displayMode !== 'modal' && c.displayMode !== 'inline' && c.displayMode !== 'popup') {
      util.log('Invalid offer displayMode; must be one of: modal, inline, popup');
      _releaseConfig(c);

      return;
    }

    if (c.displayMode === 'modal' && !c.isPreload && containerUtils.hasModalIframe()) {
      util.log('Modal offer is already shown; must be one modal offer per time');
      _releaseConfig(c);

      return;
    }

    var singleContainer;
    if (c.displayMode !== 'modal') {
      if (!c.isPreload) {
        var iframe;

        $(c.containerSelector).each(function (index, singleContainer) {
          var _c = merge({}, c);
          _c.singleContainer = singleContainer;
          iframe = _createIframe(_c);
        });

        return iframe; // returns last created iframe
      }

      var container = $(c.containerSelector).last();
      if (container) {
        singleContainer = container.length ? container[0] : undefined;
        c.singleContainer = singleContainer;
      }
    }

    return _createIframe(c);
  };


  /**
   * Create iframe with params
   *
   * @param config
   * @private
   */
  function _createIframe(config, containerWrapper) {
    var iframeParams = _getConfigurationParameters(config);

    var tbc = util.getTbcCookie();
    if (tbc && tbc.length > 0) {
      iframeParams['tbc'] = tbc;
    }

    if (config.generateIframeIdFn) {
      iframeParams.iframeId = config.generateIframeIdFn(config, iframeCounterByTemplateParamsMap);
    } else {
      iframeParams.iframeId = 'offer-' + (frameIdCount++) + '-' + util.randomString();
    }

    iframeParams.url = util.prepareUrlParameter(window.location.href);
    iframeParams.parentDualScreenLeft = window.screenLeft !== undefined ? window.screenLeft : screen.left;
    iframeParams.parentDualScreenTop = window.screenTop !== undefined ? window.screenTop : screen.top;
    iframeParams.parentWidth = window.innerWidth ? window.innerWidth : document.documentElement.clientWidth ? document.documentElement.clientWidth : screen.width;
    iframeParams.parentHeight = window.innerHeight ? window.innerHeight : document.documentElement.clientHeight ? document.documentElement.clientHeight : screen.height;
    iframeParams.parentOuterHeight = window.parent.outerHeight;

    if (config.previewNonLoggedUser) {
      iframeParams.previewNonLoggedUser = config.previewNonLoggedUser;
    }

    if (config.isPreload) {
      iframeParams.preload = 1;
    }

    if (tp.locale) {
      iframeParams.lang = tp.locale;
    }

    if (!!gaService.getClientId()) {
      iframeParams.gaClientId = gaService.getClientId();
    }

    if (!!gaService.getGAAccount()) {
      iframeParams.gaAccount = gaService.getGAAccount();
    }

    _getTpConfigurationParameters(iframeParams, tp);

    var hasLoginRequiredCallback = eventManager.hasCallback('loginRequired');

    // we can not detect loginRequired event which was added as tp.addHandler("loginRequired", function(){})
    // because composer adds empty loginRequired and we can not use this type of event.
    if (tp.janrain.isConfigured() && (typeof (config.loginRequired) === 'undefined' || !config.loginRequired)) {
      var hiddenIframes = [];

      tp.janrain.addEventHandler('onModalClose', function () {
        containerUtils.activateContainers(hiddenIframes);
      });

      setUpJanrainCallbacks();

      config.loginRequired = function (params) {
        hiddenIframes = containerUtils.hideModalContainer();
        loginRequiredAllowReturnToStartState = params.allowReturnToStartState;
        needReloadOnStartCheckout = false;

        loginRequiredTermId = params.termId;
        loginRequiredIframeId = iframeParams.iframeId;

        util.waitCall('refreshUserToken', function () {
          tp.janrain.login(null, params);
        });

        return false;
      }
    } else if (tp.gigya.isConfigured() && (typeof (config.loginRequired) === 'undefined' || !config.loginRequired)) {
      var hiddenIframes = [];

      tp.gigya.registerCallback('onHide', function (params) {
        containerUtils.activateContainers(hiddenIframes);
        postmessage.send('gigyaHide', params, loginRequiredIframeId);
      });

      setUpGigyaCallbacks();

      config.loginRequired = function (params) {
        loginRequiredAllowReturnToStartState = params.allowReturnToStartState;
        needReloadOnStartCheckout = false;
        hiddenIframes = containerUtils.hideModalContainer();
        loginRequiredTermId = params.termId;
        loginRequiredIframeId = iframeParams.iframeId;

        util.waitCall('refreshUserToken', function () {
          tp.gigya.login();
        });

        return false;
      };
    } else if (tp.pianoId.isConfigured()) {
      if ((typeof (config.loginRequired) === 'undefined' || !config.loginRequired)) {
        config.loginRequired = function (params) {
          if (params.preventExternalLogin) {
            return;
          }
          loginRequiredTermId = params.termId;
          loginRequiredIframeId = iframeParams.iframeId;

          tp.pianoId.show({ screen: params.startScreen });
          return false;
        };
      }

      setUpPianoIdCallbacks();

      iframeParams.pianoIdUrl = tp.pianoId.getIframeUrl();
      var lang = tp.pianoId.getLang();
      if (lang) {
        iframeParams.lang = lang;
      }
      tp.pianoId.onLangChange(function (lang) {
        postmessage.send('langChanged', { lang: lang }, iframeParams.iframeId);
      }, iframeParams.iframeId);
    } else if (tp.pianoIdLite.isConfigured()) {
      iframeParams.pianoIdUrl = tp.pianoId.getIframeUrl();
    }

    iframeParams.userProvider = user.getProvider().getName();
    if (config.userToken) {
      iframeParams.userToken = config.userToken;
    } else {
      iframeParams.userToken = user.getProvider().getToken() && user.getProvider().getToken().length > 0 ? user.getProvider().getToken() : '';
    }

    util.getCampaigns(iframeParams);
    iframeParams.customCookies = JSON.stringify(util.getCustomCookies());

    eventManager.registerCallback(config, iframeParams.iframeId);

    iframeParams.hasLoginRequiredCallback = eventManager.hasIframeCallback('loginRequired', iframeParams.iframeId) || hasLoginRequiredCallback;

    setIframeParamsFromQueryParams(iframeParams);

    var iframeUrl = config.iframeUrl ? config.iframeUrl : (iframeParams.isCheckout ? _getCheckoutBaseUrl() : _getOfferBaseUrl());
    var postMessageUrl = config.postMessageUrl ? config.postMessageUrl : _getOfferBaseUrl();
    var statParams = extractStatParams(iframeParams, config);

    var iframe = containerUtils.createContainer({
      url: iframeUrl,
      config: config,
      parent: config.parent,
      postMessageUrl: postMessageUrl,
      receive: _receive,
      iframeParams: iframeParams,
      singleContainer: config.singleContainer,
      close: function () {
        eventManager.fireCallbacks('close', { state: closeState }, iframeParams.iframeId, true);
      }
    });

    iframe.statParams = statParams;
    iframe.browserId = config.browserId;
    iframes.push(iframe);
    return iframe;
  }

  function setIframeParamsFromQueryParams(iframeParams) {
    var termId = util.getQueryParamByName('termId');
    var paySource = util.getQueryParamByName('paySource');
    if (termId) {
      iframeParams.termId = termId;
    }
    if (paySource) {
      iframeParams.paySource = paySource;
    }
  }

  function extractStatParams(iframeParams, config) {
    var result = {};

    if (config.trackStatsOnShow) {
      return result;
    }

    var statsParams = [
      'tbc',
      'trackingId',
      'pageViewId',
      'visitId',
      'tags',
      'contentCreated',
      'contentSection',
      'contentAuthor'
    ];

    $.each(statsParams, function (index, value) {
      if (iframeParams[value]) {
        result[value] = iframeParams[value];
        delete iframeParams[value];
      }
    });

    $.each(iframeParams, function (index, value) {
      if (index.indexOf('tpcc_') === 0) {
        result[index] = value;
        delete iframeParams[index];
      }
    });
    return result;
  }

  function trackStat(iframe) {
    var params = merge(iframe.iframeParams, iframe.statParams);
    postmessage.send('trackStat', params, iframe.iframeId);
  }

  function passBrowserId(iframe) {
    var params = {
      browserId: iframe.browserId
    };

    postmessage.send('passBrowserId', params, iframe.iframeId);
  }

  function merge(firstObj, secondObj) {
    var result = {};
    $.each(firstObj, function (index, value) {
      result[index] = value;
    });

    $.each(secondObj, function (index, value) {
      result[index] = value;
    });
    return result;
  }


  function setUpGigyaCallbacks() {
    if (!isAddedUserProviderCallbacks) {
      tp.gigya.registerCallback('userChanged', function (result) {
        if (result && result.user_token) {
          var params = {
            token_list: result.access_token.value,
            user_token: result.user_token,
            userToken: result.user_token,
            cookie_domain: result.access_token.cookie_domain,
            user: result.user,
            allowReturnToStartState: loginRequiredAllowReturnToStartState,
            termId: loginRequiredTermId
          };

          if (loginRequiredIframeId) {
            var iframe = containerUtils.getIframe(loginRequiredIframeId);

            if (iframe) {
              postmessage.send('externalLoginSuccess', params, loginRequiredIframeId);
              delete params.termId;
              _reloadAll(params, loginRequiredIframeId);
            } else {
              _reloadAll(params);
            }
          } else {
            _reloadAll(params);
          }
        }
      });

      tp.gigya.registerCallback('logout', function () {
        var config = {};
        config.userProvider = user.getProvider().getName();
        config.userToken = user.getProvider().getToken() && user.getProvider().getToken().length > 0 ?
          user.getProvider().getToken() : '';
        config.noNeedStartCheckout = true;

        var iframe = containerUtils.getIframe(loginRequiredIframeId);

        if (iframe) {
          if (iframe.config.displayMode === 'inline') {
            containerUtils.closeModalContainer();
          }
        }

        loginRequiredTermId = null;
        loginRequiredIframeId = null;
        loginRequiredAllowReturnToStartState = false;

        _reloadAll(config);
      });
      isAddedUserProviderCallbacks = true;
    }
  }

  function setUpJanrainCallbacks() {
    if (!isAddedUserProviderCallbacks) {
      tp.janrain.registerCallback('userChanged', function onUserChanged (result) {
        if (result && result.user_token) {
          var params = {
            token_list: result.access_token.value,
            user_token: result.user_token,
            userToken: result.user_token,
            cookie_domain: result.access_token.cookie_domain,
            user: result.user,
            allowReturnToStartState: loginRequiredAllowReturnToStartState,
            termId: loginRequiredTermId
          };


          if (loginRequiredIframeId) {
            var iframe = containerUtils.getIframe(loginRequiredIframeId);

            if (iframe) {
              postmessage.send('externalLoginSuccess', params, loginRequiredIframeId);
              delete params.termId;
              _reloadAll(params, loginRequiredIframeId);
            } else {
              _reloadAll(params);
            }
          } else {
            _reloadAll(params);
          }
        }
      });

      tp.janrain.addEventHandler('onCaptureSessionEnded', function () {
