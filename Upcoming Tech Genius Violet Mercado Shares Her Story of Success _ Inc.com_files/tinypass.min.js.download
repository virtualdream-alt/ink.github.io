tp.define('fbsubscriptions',[
  'jq',
  'util',
  'fbpixel',
  'offer',
  'pianoId',
  'user',
  'api'
], function (
  $,
  util,
  fbpixel,
  offer,
  pianoId,
  user,
  api
) {

  var isInitialized = false;
  var signedSubscriptionPayload = '';
  var sentEvents = {};
  var sentCustomEvents = {};

  function _init() {
    if (isInitialized) {
      return;
    }

    fbpixel.__protected__.init();

    offer.registerCallback('loaded', function () {
      var surfaceParameter = util.getQueryParamByName('surface');
      if ((surfaceParameter === 'meter_limit_reached' || surfaceParameter === 'locked_article') && !tp.inappBrowserReturn) {
        trackCustomEvent('ViewPaywall',
          {
            surface: 'IA'
          });
      }
    });

    var loginSuccessCallback = function (params) {
      _generateAndSignSubscriptionPayload().then(function () {
        if (params.registration) {
          trackCustomEvent('CreateAccount');
        } else {
          trackCustomEvent('LogIntoAccount', { is_subscriber: !!signedSubscriptionPayload });
        }
        if (!!signedSubscriptionPayload) {
          // wait for the pixel to complete according to
          // https://developers.facebook.com/docs/mediaguide/pixel-and-analytics?locale=ru_RU
          setTimeout(function () {
            _redirectToFacebook();
          }, 200);
        }
      });
    };

    var experienceExecuteActionHandler = function () {
      if (user.isUserValid()) {
        _redirectIfUserHasAccess();
      }
    };

    tp.addHandler('loginSuccess', loginSuccessCallback);
    tp.addHandler('experienceExecute', experienceExecuteActionHandler);

    offer.registerCallback('alreadyPurchased', function (params) {
      _checkResourceAccess(params.rid);
    });

    offer.registerCallback('startCheckout', function (params) {
      trackEvent('InitiateCheckout',
        {
          offer_code: params.offerId,
          campaign_code: params.termId
        });
    });

    offer.registerCallback('complete', function (params) {
      if (params.hasOwnProperty('chargeCurrency') && params.hasOwnProperty('chargeAmount')) {
        trackEvent('Purchase',
          {
            currency: params.chargeCurrency,
            value: params.chargeAmount,
            offer_code: params.offerId,
            campaign_code: params.termId
          });
      }
      _checkResourceAccess(params.rid);
    });

    offer.registerCallback('close', function () {
      _redirectToFacebook();
    });

    isInitialized = true;
  }

  function resolveTimeout() {
    var def = $.Deferred();

    setTimeout(function () {
      def.resolve();
    }, tp.redirectToFbTimeout * 1000);

    return def.promise();
  }

  function _generateAndSignSubscriptionPayload(rid) {
    var def = $.Deferred();

    var requestParams = {
      aid: tp.aid,
      rid: '',
      account_linking_token: util.getQueryParamByName('account_linking_token'),
      user_token: user.getProvider().getToken(),
      user_provider: user.getProvider().getName()
    };

    if (rid) {
      requestParams.rid = rid;
    }

    api.callApi('/fbia/subscriptionPayload/generate', requestParams,
      function (response) {
        if (response && response.data) {
          signedSubscriptionPayload = response.data;
        }
        def.resolve();
      }
    );

    return def.promise();
  }

  function _redirectIfUserHasAccess() {
    _generateAndSignSubscriptionPayload().then(function () {
      if (signedSubscriptionPayload) {
        _redirectToFacebook();
      }
    });
  }

  function _checkResourceAccess(rid) {
    if (!rid) {
      util.error('fbsubscriptions: \'rid\' could not be empty on generating subscription payload');
      return;
    }

    var timerPromise = resolveTimeout();
    var generatePayloadPromise = _generateAndSignSubscriptionPayload(rid);
    util.__protected__.waitAllPromises([timerPromise, generatePayloadPromise]).then(function () {
      _redirectToFacebook();
    });
  }

  function _redirectToFacebook() {
    var accountLinkingToken = util.getQueryParamByName('account_linking_token');
    var redirectUri = util.getQueryParamByName('redirect_uri') + "?account_linking_token=" + encodeURIComponent(accountLinkingToken);
    if (signedSubscriptionPayload) {
      redirectUri += '&subscription_payload=' + encodeURIComponent(signedSubscriptionPayload);
    }
    window.location = redirectUri;
  }

  function _checkUserAccessAndRedirectToFb() {
    _generateAndSignSubscriptionPayload().then(function () {
      _redirectToFacebook();
    });
  }

  function trackEvent(eventName, eventParams) {
    if (sentEvents[eventName]) {
      return;
    }
    sentEvents[eventName] = true;


    fbpixel.trackEvent(eventName, eventParams);
  }

  function trackCustomEvent(eventName, eventParams) {
    if (sentCustomEvents[eventName]) {
      return;
    }
    sentCustomEvents[eventName] = true;

    fbpixel.trackCustomEvent(eventName, eventParams);
  }

  var _isInitialized = function () {
    return isInitialized;
  };

  return {
    name: 'fbsubscriptions',
    /**
     * Fire this method after loginRequired in case user_ref
     *
     * tp.push(["addHandler", "loginRequired", function(params){
     *   mysite.showLoginRegistration(function() {
     *     tp.push(["setUserRef", pianoUserRef]);
     *     tp.fbsubscriptions.checkUserAccessAndRedirectToFb();
     *   });
     *
     *   return false;
     * }
     */
    checkUserAccessAndRedirectToFb: _checkUserAccessAndRedirectToFb,
    isInitialized: _isInitialized,
    __private__: {
      isInitialized: _isInitialized
    },
    __protected__: {
      init: _init
    }
  };
});

