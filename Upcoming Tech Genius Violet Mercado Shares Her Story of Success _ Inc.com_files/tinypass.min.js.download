tp.define('performanceMetrics',['postmessage'], function (postmessage) {
  var entriesToFollow = [
    {
      resourceRegExp: new RegExp('/checkout/offer/show'),
      label: 'showOffer'
    },
    {
      resourceRegExp: new RegExp('tinypass\.min\.js$'),
      label: 'tinypassMinJS'
    },
    {
      resourceRegExp: new RegExp('/xbuilder/experience/execute'),
      label: 'experienceExecute'
    }
  ];

  var isLoggerReady = false;
  var loggerIframeId;
  var batch = [];

  var EVENT_TP_PERFORMANCE_DATA = 'EVENT_TP_PERFORMANCE_DATA';
  var EVENT_LOGGER_READY = 'EVENT_LOGGER_READY';

  init();

  function init() {
    if (!window.performance || !window.PerformanceObserver) {
      return;
    }

    if (!(typeof performance.getEntries === 'function')) {
      return;
    }

    var observe = window.PerformanceObserver.prototype.observe;
    window.PerformanceObserver.prototype.observe = function () {
      try {
        observe.apply(this, arguments);
      } catch (e) {
      }
    };

    // waiting for handshake
    postmessage.subscribe(EVENT_LOGGER_READY, function (data) {
      loggerIframeId = data.sender;
      isLoggerReady = true;

      execBatch();
    });

    // handle existing entries
    var existingEntries = performance.getEntries();
    onEntriesArrive(existingEntries);

    // observe future entries
    var observer = new PerformanceObserver(function (list) {
      onEntriesArrive(list.getEntries());
    });

    observer.observe({
      entryTypes: ['measure', 'resource']
    });
  }

  function onEntriesArrive(entries) {
    entries.forEach(function (entry) {
      onEntryArrive(entry);
    })
  }

  /*
  * Performance entry
  * https://developer.mozilla.org/ru/docs/Web/API/Performance
  * */
  function onEntryArrive(entry) {
    if (!shouldWatch(entry)) {
      return;
    }

    emitEntry(entry);
  }

  function emitEntry(entry) {
    if (!isReady()) {
      batch.push(entry);
      return;
    }

    batch.push(entry);
    execBatch();
  }

  function execBatch() {
    if (!isReady()) {
      return;
    }

    batch.forEach(function (_entry) {
      postmessage.send(EVENT_TP_PERFORMANCE_DATA, {
        entry: _entry
      }, loggerIframeId);
    });

    batch = [];
  }

  function isReady() {
    return isLoggerReady && loggerIframeId;
  }

  function shouldWatch(entry) {
    return entriesToFollow.some(function (_entryToFollow) {
      return entry.name.match(_entryToFollow.resourceRegExp);
    });
  }

  return {
    name: 'performanceMetrics'
  };
});

tp.define('swg', ['postmessage', 'user', 'jq', 'util', 'eventUtils'],
  function (postmessage, user, $, util, eventUtils) {

    var EXTERNAL_CHECKOUT_COMPLETE_EVENT = 'externalCheckoutComplete';
    var SwgUiFlows = {
      LOGIN_PROMPT: 'loginPrompt',
      DEFERRED_ACCOUNT_CREATION: 'deferredAccountCreation',
      BUY: 'buy'
    };

    var eventManager = eventUtils.createEventManager([
      EXTERNAL_CHECKOUT_COMPLETE_EVENT
    ]);

