            }
        }

        return true;
    }

    function showLogin(config) {
        config = config ? config : {};
        var provider = user.getProvider();
        if (provider.getName() === pianoId.getName()) {
            var onBindUserWithReaderId;
            if (config.loginSuccess && $.isFunction(config.loginSuccess)) {
                onBindUserWithReaderId = config.loginSuccess;
            } else {
                onBindUserWithReaderId = goToReturnUrl;
            }
            var loginSuccess = function() {
                me.__private__.bindUserWithReaderId({}, onBindUserWithReaderId);
            };
            var showConfig = $.extend({}, me.__private__.globalConfig, config);

            if (user.isUserValid()) {
                loginSuccess();
                return;
            }

            showConfig.loginSuccess = loginSuccess;

            pianoId.show(showConfig);
        } else {
            util.log("Current user provider" + provider.name + " is not allowed for amp");
            return;
        }
    }

    function logout(callback) {
        callback = $.isFunction(callback) ? callback : goToReturnUrl;
        var provider = user.getProvider();
        if (provider.getName() === pianoId.getName()) {
            pianoId.logout(function() {
                me.__private__.unbindUserWithReaderId(callback);
            });
        } else {
            me.__private__.unbindUserWithReaderId(callback);
        }
    }

    function isUserStateNameValid(name) {
        return /^\w+$/i.test(name);
    }

    function login(config) {
        var userProvider = user.getProvider().getName();
        if (userProvider === pianoId.getName()) {
            if (!config) {
                config = {
                  userProvider: userProvider
                };
            } else {
                config = $.extend(true, config,  {
                    userProvider: userProvider
                });
            }
        }

        var loginConfig = {
            user_transient: true,
            user_provider: me.__private__.composerOnlyProvider
        };

        if (config && config.readerId) {
            loginConfig.reader_id = config.readerId;
        }

        if (config && config.userToken) {
            loginConfig.user_token = config.userToken;
        }

        if (config && config.userProvider) {
            loginConfig.user_provider = config.userProvider;
            loginConfig.user_transient = false;
        }

        if (!validUserState(config)) {
            return false;
        }

        if (config && config.userState) {
            loginConfig.user_state = JSON.stringify(config.userState);
        }

        var apiCallback = function(response) {
            if (response.code === 0) {
                if (config.loginSuccess && $.isFunction(config.loginSuccess)) {
                    config.loginSuccess();
                }
            } else {
                if (config.loginFail && $.isFunction(config.loginFail)) {
                    config.loginFail();
                }
            }
        };

        me.__private__.bindUserWithReaderId(loginConfig, apiCallback);
    }

    function unbindUserWithReaderId(callback) {
        var apiConfig = {};
        apiConfig.reader_id = util.getQueryParamByName(me.__private__.READER_ID);

        if (!apiConfig.reader_id) {
            util.log('reader_id cannot be empty');
            return;
        }

        api.callApi(me.__private__.unbindURL, apiConfig, callback);
    }

    function bindUserWithReaderId(config, callback) {
        var apiConfig = config || {};

        if (!apiConfig.reader_id) {
            apiConfig.reader_id = util.getQueryParamByName(me.__private__.READER_ID);
        }

        if (!apiConfig.reader_id) {
            util.log('reader_id cannot be empty');
            return;
        }

        api.callApi(me.__private__.bindURL, apiConfig, callback);
    }

    return me;
});

