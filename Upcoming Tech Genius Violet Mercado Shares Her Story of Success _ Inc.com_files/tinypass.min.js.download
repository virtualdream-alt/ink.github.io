      switch (message.event) {
        case "newsletterSignupSubmission":
          var storedMailingLists = loadCompleteMailingLists();
          if (storedMailingLists.indexOf(message.params.mailingListIds) === -1) {
            storedMailingLists.push(message.params.mailingListIds);
            saveCompleteMailingLists(storedMailingLists);
          }
          break;
      }
    };

    var getPlaceholderFn = function (holdername) {
      return function () {
        return $('<pnespwgtplaceholder holdername="' + holdername + '"></pnespwgtplaceholder>')
      };
    };

    var getWidgetCallbacks = function () {
      return {
        onrecsloaded: function (widget) {
          return function (data) {
            if (data.countOfRecs === 0) {
              recWidget.closeWidget({ widget: widget, skipOnclose: true });
              return;
            }

            recWidget.fixWidgetModalWidth(widget, true);

            log.__protected__.logEspMicroConversion(widget.trackingId, widget.widgetId, 'init', EXTERNAL_EVENT);
            util.debug('creating recommendation widget: ', widget.config);
          }
        },
        onclick: function (widget) {
          return function (data) {
            var event_params = {
              href: data.url
            };

            log.__protected__.logEspMicroConversion(widget.trackingId, widget.widgetId, 'click', EXTERNAL_LINK, event_params);
          }
        },
        onclose: function (widget) {
          return function () {
            var event_params = {
              event: 'widgetClosed'
            };

            log.__protected__.logEspMicroConversion(widget.trackingId, widget.widgetId, 'close', EXTERNAL_EVENT, event_params);
          }
        }
      };
    }

    var removeWidgetsAndSdk = function () {
      recWidget.clearWidgets();
      initPromise = null;
      delete window.PianoESP;
    };

    var onInit = function (callback) {
      if (!initPromise) {
        return;
      }

      if (typeof callback === 'function') {
        initPromise.then(callback);
      }
    };

    setTimeout(function() {
      // logout listener for tinypassLogin
      tp.addHandler('logout', function () {
        saveCompleteMailingLists([]);
      });

      // logout listener for other providers
      var provider = user.getProvider();

      if (provider.registerCallback) {
        provider.registerCallback('logout', function () {
          saveCompleteMailingLists([]);
        });
      }
    }, USER_PROVIDERS_INIT_TIMEOUT);

    return {
      name: 'pianoEsp',
      __protected__: {
        init: _init,
        setVisitorId: _setVisitorId,
        showRecommendation: _showRecommendation,
        showPushSignup: _showPushSignup,
        getNotificationStatus: _getNotificationStatus,
        showNewsletterSignup: showNewsletterSignup,
        offerMessageReceiver: offerMessageReceiver
      },
      __private__: {
        removeWidgetsAndSdk: removeWidgetsAndSdk,
        getWidgetCallbacks: getWidgetCallbacks,
        onInit: onInit
      }
    };
  });

